            "value": 200,
            "unit": "g"
          },
          "duration": {
            "value": 5,
            "unit": "min"
          },
          "temperature": {
            "value": 4,
            "unit": "°C"
          }
        },
        "annotations": {}
      },
      {
        "id": "s5",
        "op": "manipulate",
        "inputs": [
          {
            "kind": "sample",
            "id": "pellet1"
          },
          {
            "kind": "material",
            "id": "adDMEM_F12_medium"
          }
        ],
        "outputs": [
          {
            "kind": "sample",
            "id": "resuspended_cells"
          }
        ],
        "params": {
          "action_kind": "resuspend"
        },
        "annotations": {
          "note": "Resuspend pellet in 10 ml adDMEM/F12 medium."
        }
      },
      {
        "id": "s6",
        "op": "annotate",
        "inputs": [
          {
            "kind": "sample",
            "id": "resuspended_cells"
          }
        ],
        "outputs": [
          {
            "kind": "sample",
            "id": "resuspended_cells"
          }
        ],
        "params": {},
        "annotations": {
          "description": "Filter using a 100‑µm cell strainer."
        }
      },
      {
        "id": "s7",
        "op": "centrifuge",
        "inputs": [
          {
            "kind": "sample",
            "id": "resuspended_cells"
          }
        ],
        "outputs": [
          {
            "kind": "sample",
            "id": "pellet2"
          }
        ],
        "params": {
