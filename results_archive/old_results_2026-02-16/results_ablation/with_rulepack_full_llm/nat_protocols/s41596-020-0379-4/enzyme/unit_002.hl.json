  "protocol": {
    "steps": [
      {
        "id": "s1",
        "op": "annotate",
        "inputs": [],
        "outputs": [],
        "params": {},
        "annotations": {
          "description": "Digest the tissue (from Step 3) as appropriate based on the tumor type (Table 1). Monitor digestion; for <2‑h incubations, mix every 10–15 min by shaking vigorously and by pipetting up/down with a P1000 pipette. For overnight incubations, place the mixture on a shaker. Addition of 10 µM ROCK inhibitor during digestion can increase outgrowth efficiency. For head and neck organoids, digestion should not exceed 1 h and should be monitored regularly."
        }
      },
      {
        "id": "s2",
        "op": "manipulate",
        "inputs": [
          {
            "kind": "sample",
            "id": "digested_mix"
          }
        ],
        "outputs": [
          {
            "kind": "sample",
            "id": "digested_mix"
          }
        ],
        "params": {
          "action_kind": "mix_gently"
        },
        "annotations": {
          "note": "Pipette up and down 10–20 times using a P1000 pipette."
        }
      },
      {
        "id": "s3",
        "op": "transfer",
        "inputs": [
          {
            "kind": "sample",
            "id": "digested_mix"
          },
          {
            "kind": "material",
            "id": "adDMEM_F12_medium"
          }
        ],
        "outputs": [
          {
            "kind": "sample",
            "id": "mixture_with_medium"
          }
        ],
        "params": {
          "amount": {
            "value": 10,
            "unit": "ml"
          }
        },
        "annotations": {}
      },
      {
        "id": "s4",
        "op": "centrifuge",
        "inputs": [
          {
            "kind": "sample",
            "id": "mixture_with_medium"
          }
        ],
        "outputs": [
