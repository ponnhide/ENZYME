      {
        "id": "s1",
        "op": "annotate",
        "inputs": [
          {
            "kind": "material",
            "id": "matrigel_aliquot"
          }
        ],
        "outputs": [
          {
            "kind": "material",
            "id": "matrigel_aliquot"
          }
        ],
        "params": {},
        "annotations": {
          "note": "Take out required amount of Matrigel aliquots from the freezer and let them thaw on ice."
        }
      },
      {
        "id": "s2",
        "op": "annotate",
        "inputs": [
          {
            "kind": "material",
            "id": "sterile_scissors"
          }
        ],
        "outputs": [
          {
            "kind": "material",
            "id": "sterile_scissors"
          }
        ],
        "params": {},
        "annotations": {
          "note": "Heat sterile scissors to at least 180 °C for at least 3 h, then let them cool at room temperature."
        }
      },
      {
        "id": "s3",
        "op": "observe",
        "inputs": [
          {
            "kind": "material",
            "id": "96U_well_plate"
          },
          {
            "kind": "equipment",
            "id": "microscope"
          }
        ],
        "outputs": [
          {
            "kind": "material",
            "id": "96U_well_plate"
          }
        ],
        "params": {
          "modality": "microscope",
          "features": "check hPSC aggregate morphology (round, homogeneous, ~550–650 µm diameter)"
        },
        "annotations": {}
      },
      {
        "id": "s4",
        "op": "annotate",
        "inputs": [
          {
            "kind": "material",
            "id": "pipette_2_20ul"
          },
          {
            "kind": "material",
            "id": "pipette_10_100ul"
          }
        ],
        "outputs": [
          {
            "kind": "material",
            "id": "pipette_2_20ul"
          },
          {
            "kind": "material",
            "id": "pipette_10_100ul"
          }
        ],
        "params": {},
        "annotations": {
          "note": "Cut tip of the 2–20 µL pipet at ~90° and set to 3 µL (for picking aggregates). Set the 10–100 µL pipet to 20 µL (for making Matrigel droplets)."
        }
      },
      {
        "id": "s5",
        "op": "transfer",
        "inputs": [
          {
            "kind": "material",
            "id": "matrigel_aliquot"
          },
          {
            "kind": "material",
            "id": "96U_well_plate"
          }
        ],
        "outputs": [
          {
            "kind": "material",
            "id": "96U_well_plate"
          }
        ],
        "params": {
          "amount": {
            "value": 20,
            "unit": "µL"
          },
          "description": "Pipet 20 µL of Matrigel into an empty well."
        },
        "annotations": {}
      },
      {
        "id": "s6",
        "op": "transfer",
        "inputs": [
          {
            "kind": "material",
            "id": "hpsc_aggregates"
          },
          {
            "kind": "material",
            "id": "96U_well_plate"
          }
        ],
        "outputs": [
          {
            "kind": "material",
            "id": "96U_well_plate"
          }
        ],
        "params": {
          "description": "Pick a properly formed aggregate with the cut tip and aspirate it."
        },
        "annotations": {}
      },
      {
        "id": "s7",
        "op": "annotate",
        "inputs": [
          {
            "kind": "material",
            "id": "96U_well_plate"
          }
        ],
        "outputs": [
          {
            "kind": "material",
            "id": "96U_well_plate"
          }
        ],
        "params": {},
        "annotations": {
          "note": "Insert the aggregate into the Matrigel droplet by depressing the pipet plunger only to the first stop; then tilt the plate to verify the aggregate is inside the droplet."
        }
      },
      {
        "id": "s8",
        "op": "observe",
        "inputs": [
          {
            "kind": "material",
            "id": "96U_well_plate"
          },
          {
            "kind": "equipment",
            "id": "microscope"
          }
        ],
        "outputs": [
          {
            "kind": "material",
            "id": "96U_well_plate"
          }
        ],
        "params": {
          "modality": "visual",
          "features": "verify aggregate is inside Matrigel droplet by tilting plate"
        },
        "annotations": {}
      },
      {
        "id": "s9",
        "op": "annotate",
        "inputs": [
          {
            "kind": "material",
            "id": "96U_well_plate"
          }
        ],
        "outputs": [
          {
            "kind": "material",
            "id": "96U_well_plate"
          }
        ],
        "params": {},
        "annotations": {
          "note": "Repeat steps s5–s8 for each properly formed aggregate, using a new tip for every Matrigel droplet. After all aggregates are embedded, place the plate into the incubator for 50–60 min to let Matrigel solidify."
        }
      },
      {
        "id": "s10",
        "op": "incubate",
        "inputs": [
          {
            "kind": "material",
            "id": "96U_well_plate"
          },
          {
            "kind": "equipment",
            "id": "incubator"
          }
        ],
        "outputs": [
          {
            "kind": "material",
            "id": "96U_well_plate"
          }
        ],
        "params": {
          "duration": {
            "value": 55,
            "unit": "min"
          },
          "description": "Incubate plate to allow Matrigel solidification."
        },
        "annotations": {}
      },
      {
        "id": "s11",
        "op": "transfer",
        "inputs": [
          {
            "kind": "material",
            "id": "e8_medium"
          },
          {
            "kind": "material",
            "id": "96U_well_plate"
          }
        ],
        "outputs": [
          {
            "kind": "material",
            "id": "96U_well_plate"
          }
        ],
        "params": {
          "amount": {
            "value": 100,
            "unit": "µL"
          },
          "description": "Add 100 µL E8 medium per well using a multichannel pipet; depress plunger only to first stop."
        },
        "annotations": {}
      },
      {
        "id": "s12",
        "op": "incubate",
        "inputs": [
          {
            "kind": "material",
            "id": "96U_well_plate"
          },
          {
            "kind": "equipment",
            "id": "incubator"
          }
        ],
        "outputs": [
          {
            "kind": "material",
            "id": "96U_well_plate"
          }
        ],
        "params": {
          "description": "Place plate back into the incubator."
        },
        "annotations": {}
      }
