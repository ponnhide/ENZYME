    "steps": [
      {
        "annotations": {
          "note": "Detach hPSCs from a T25 flask at 80–90% confluence by Accutase treatment and count the cells as described in Steps 2–5."
        },
        "id": "s1",
        "inputs": [
          {
            "id": "s_hPSC",
            "kind": "sample"
          }
        ],
        "op": "annotate",
        "outputs": [
          {
            "id": "s_hPSC",
            "kind": "sample"
          }
        ],
        "params": {}
      },
      {
        "annotations": {
          "note": "Prepare a stock cell suspension at a concentration of 5,000 cells per 100 µL medium by adding an appropriate amount of cells from Step 10 to E8 + 10 µM Y27632. Gently resuspend the cells."
        },
        "id": "s2",
        "inputs": [
          {
            "id": "s_hPSC",
            "kind": "sample"
          },
          {
            "id": "m_E8",
            "kind": "material"
          },
          {
            "id": "m_Y27632",
            "kind": "material"
          }
        ],
        "op": "annotate",
        "outputs": [
          {
            "id": "s_stock",
            "kind": "sample"
          }
        ],
        "params": {}
      },
      {
        "annotations": {
          "note": "Seed 100 µL of the stock cell suspension per well of the U‑shaped ultra‑low attachment 96‑well plate, seeding 12 extra wells (one row) beyond the desired number of HFOs."
        },
        "id": "s3",
        "inputs": [
          {
            "id": "s_stock",
            "kind": "sample"
          },
          {
            "id": "c_96well",
            "kind": "container"
          }
        ],
        "op": "transfer",
        "outputs": [
          {
            "id": "s_seeded_plate",
            "kind": "sample"
          }
        ],
        "params": {
          "amount": {
            "unit": "µL",
            "value": 100
          }
        }
      },
      {
        "annotations": {},
        "id": "s4",
        "inputs": [
          {
            "id": "s_seeded_plate",
            "kind": "sample"
          }
        ],
        "op": "run_device",
        "outputs": [
