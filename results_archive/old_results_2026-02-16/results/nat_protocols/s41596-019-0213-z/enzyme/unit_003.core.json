  "steps": [
    {
      "id": "s1",
      "inputs": [
        "e1",
        "e2"
      ],
      "notes": "Wash hPSCs with 0.5 mM EDTA by removing E8 medium.",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "action": "remove E8 medium",
        "container": "e20"
      }
    },
    {
      "id": "s2",
      "inputs": [
        "e3"
      ],
      "notes": "Add 0.6 mL of 0.5 mM EDTA per well of a 6‑well plate.",
      "op": "transfer",
      "outputs": [],
      "params": {
        "container": "e20",
        "volume": "0.6 mL per well"
      }
    },
    {
      "id": "s3",
      "inputs": [
        "e3"
      ],
      "notes": "",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "action": "aspirate EDTA solution",
        "container": "e20"
      }
    },
    {
      "id": "s4",
      "inputs": [
        "e4"
      ],
      "notes": "Add Accutase.",
      "op": "transfer",
      "outputs": [],
      "params": {
        "container": "e20",
        "volume": "0.6 mL per well"
      }
    },
    {
      "id": "s5",
      "inputs": [],
      "notes": "Incubate Accutase for 3–5 min at 37 °C. Monitor cells under microscope until they round up.",
      "op": "incubate",
      "outputs": [],
      "params": {
        "duration": "3–5 min",
        "temperature": "37 °C"
      }
    },
    {
      "id": "s6",
      "inputs": [
        "e1"
      ],
      "notes": "Closely monitor cells under the tissue culture microscope until the cells start to round up.",
      "op": "observe",
      "outputs": [],
      "params": {
        "instrument": "e29"
      }
    },
    {
      "id": "s7",
      "inputs": [
        "e1"
      ],
      "notes": "Dissociate the cells with a P1000 pipette and pipette gently up and down (2–3×).",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "action": "dissociate cells",
        "instrument": "e32",
        "repetitions": "2–3×"
      }
    },
    {
      "id": "s8",
      "inputs": [
        "e5"
      ],
      "notes": "Resuspend cells in 5 mL aggregation medium using a 5‑mL serological pipette and collect in a 15‑mL Falcon tube.",
      "op": "transfer",
      "outputs": [
        "e23"
      ],
      "params": {
        "destination": "e23",
        "instrument": "e24",
        "volume": "5 mL"
      }
    },
    {
      "id": "s9",
      "inputs": [
        "e1",
        "e19"
      ],
      "notes": "Mix 10 μL of cell suspension and 10 μL of 0.1% Trypan blue.",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "instrument": "e32",
        "volume1": "10 μL cell suspension",
        "volume2": "10 μL 0.1% Trypan blue"
      }
    },
    {
      "id": "s10",
      "inputs": [],
      "notes": "Pipette 10 μL of the mixture onto a counting chamber slide.",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "destination": "counting chamber slide",
        "instrument": "e32",
        "volume": "10 μL"
      }
    },
    {
      "id": "s11",
      "inputs": [],
      "notes": "Count viable cells using an automated cell counter or under a microscope.",
      "op": "observe",
      "outputs": [],
      "params": {
        "instrument": "e29"
      }
    },
    {
      "id": "s12",
      "inputs": [],
      "notes": "Calculate the cell suspension volume needed for the experiment (2 × 10⁵ cells per well). Adjust for lines that form poor aggregates (5 × 10⁵–1 × 10⁶ cells per well).",
      "op": "annotate",
      "outputs": [],
      "params": {}
    },
    {
      "id": "s13",
      "inputs": [
        "e23"
      ],
      "notes": "Centrifuge the cells at 300 g for 5 min (22–23 °C) and aspirate the medium.",
      "op": "centrifuge",
      "outputs": [],
      "params": {
        "duration": "5 min",
        "speed": "300 g",
        "temperature": "22–23 °C"
      }
    },
    {
      "id": "s14",
      "inputs": [
        "e5",
        "e6"
      ],
      "notes": "Resuspend the cells in fresh aggregation medium (3 mL per well) containing 50 µM Y‑27632.",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "container": "e21",
        "volume_per_well": "3 mL"
      }
    },
    {
      "id": "s15",
      "inputs": [],
      "notes": "Place 3 mL of cell suspension per well into a 6‑well low‑attachment plate.",
      "op": "transfer",
      "outputs": [],
      "params": {
        "destination": "e21",
        "volume_per_well": "3 mL"
      }
    },
    {
      "id": "s16",
      "inputs": [],
      "notes": "Incubate for 1 day until cell aggregates with smooth borders (>50–100 µm) have formed.",
      "op": "incubate",
      "outputs": [],
      "params": {
        "duration": "1 d",
        "temperature": "37 °C"
      }
    },
    {
      "id": "s17",
      "inputs": [],
      "notes": "One day after aggregates have formed (d0), transfer the aggregates to a 15‑mL Falcon tube with a 10‑mL serological pipette and let settle by gravitation.",
      "op": "transfer",
      "outputs": [],
      "params": {
        "destination": "e23",
        "instrument": "e25"
      }
    },
    {
      "id": "s18",
      "inputs": [],
      "notes": "Do not discard the 6‑well plate; save it for later aggregate replating.",
      "op": "annotate",
      "outputs": [],
      "params": {}
    },
    {
      "id": "s19",
      "inputs": [],
      "notes": "After aggregates have collected at the bottom of the 15‑mL Falcon tube (15–20 min), carefully aspirate the medium.",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "action": "allow aggregates to settle",
        "duration": "15–20 min"
      }
    },
    {
      "id": "s20",
      "inputs": [
        "e7",
        "e8",
        "e9"
      ],
      "notes": "Resuspend aggregates (3 mL per well) in N2B27 medium + 12 µM CHIR99021 + 30 ng/mL BMP‑4 and transfer back to the low‑attachment plate.",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "destination": "e21",
        "volume_per_well": "3 mL"
      }
    },
    {
      "id": "s21",
      "inputs": [],
      "notes": "Use a P1000 pipette to quickly resuspend the aggregates once per day to avoid excessive fusion.",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "frequency": "once per day",
        "instrument": "e32"
      }
    },
    {
      "id": "s22",
      "inputs": [],
      "notes": "Incubate for 3 days to induce mesodermal differentiation.",
      "op": "incubate",
      "outputs": [],
      "params": {
        "duration": "3 d",
        "temperature": "37 °C"
      }
    },
    {
      "id": "s23",
      "inputs": [],
      "notes": "On day 3, transfer the aggregates to a 15‑mL tube and let settle by gravitation.",
      "op": "transfer",
      "outputs": [],
      "params": {
        "destination": "e23",
        "instrument": "e25"
      }
    },
    {
      "id": "s24",
      "inputs": [],
      "notes": "After aggregates have collected at the bottom, carefully aspirate the medium.",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "action": "aspirate medium"
      }
    },
    {
      "id": "s25",
      "inputs": [
        "e7",
        "e10",
        "e11"
      ],
      "notes": "Resuspend aggregates (3 mL per well) in N2B27 medium + 100 ng/mL VEGF‑A + 2 µM forskolin and transfer back to low‑attachment plates.",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "destination": "e21",
        "volume_per_well": "3 mL"
      }
    },
    {
      "id": "s26",
      "inputs": [],
      "notes": "Incubate for 2 more days.",
      "op": "incubate",
      "outputs": [],
      "params": {
        "duration": "2 d",
        "temperature": "37 °C"
      }
    },
    {
      "id": "s27",
      "inputs": [],
      "notes": "Calculate the number of wells of a 12‑well plate needed for aggregate embedding (≈40–60 aggregates per well).",
      "op": "annotate",
      "outputs": [],
      "params": {}
    },
    {
      "id": "s28",
      "inputs": [],
      "notes": "Calculate total collagen I–Matrigel solution needed (1 mL per well, 0.5 mL layer 1 + 0.5 mL layer 2).",
      "op": "annotate",
      "outputs": [],
      "params": {}
    },
    {
      "id": "s29",
      "inputs": [
        "e14"
      ],
      "notes": "Thaw the required Matrigel aliquots on ice.",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "action": "thaw on ice"
      }
    },
    {
      "id": "s30",
      "inputs": [
        "e12",
        "e13"
      ],
      "notes": "Pipette the collagen I solution as described and mix.",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "action": "mix by pipetting up and down after adding PureCol"
      }
    },
    {
      "id": "s31",
      "inputs": [
        "e12"
      ],
      "notes": "Pipette the exact amount of collagen I solution into a fresh 15‑mL Falcon tube and place on ice.",
      "op": "transfer",
      "outputs": [
        "e23"
      ],
      "params": {
        "action": "place on ice",
        "destination": "e23",
        "instrument": "e25"
      }
    },
    {
      "id": "s32",
      "inputs": [
        "e14"
      ],
      "notes": "",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "action": "add Matrigel to collagen I solution and mix thoroughly"
      }
    },
    {
      "id": "s33",
      "inputs": [],
      "notes": "Add 0.5 mL of collagen I/Matrigel solution per 12‑well plate and swirl to distribute.",
      "op": "transfer",
      "outputs": [],
      "params": {
        "destination": "e22",
        "volume": "0.5 mL per well"
      }
    },
    {
      "id": "s34",
      "inputs": [],
      "notes": "Incubate the plate at 37 °C for 2 h for solidification.",
      "op": "incubate",
      "outputs": [],
      "params": {
        "duration": "2 h",
        "temperature": "37 °C"
      }
    },
    {
      "id": "s35",
      "inputs": [],
      "notes": "Pipette the aggregates into 15‑mL tubes with a 10‑mL serological pipette. Collect aggregates by gravitation.",
      "op": "transfer",
      "outputs": [],
      "params": {
        "destination": "e23",
        "instrument": "e25"
      }
    },
    {
      "id": "s36",
      "inputs": [
        "e15"
      ],
      "notes": "Wash the aggregates by aspirating the supernatant and adding 1 mL StemPro‑34 SFM complete medium. Repeat once.",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "action": "wash aggregates",
        "repetitions": "2",
        "volume": "1 mL"
      }
    },
    {
      "id": "s37",
      "inputs": [],
      "notes": "Use a P200 pipette to aspirate the medium; remove all remaining medium carefully.",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "action": "aspirate remaining medium",
        "instrument": "e33"
      }
    },
    {
      "id": "s38",
      "inputs": [],
      "notes": "For the second layer, add PureCol to prepare complete collagen I solution and keep on ice.",
      "op": "annotate",
      "outputs": [],
      "params": {}
    },
    {
      "id": "s39",
      "inputs": [],
      "notes": "",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "action": "resuspend aggregates in collagen I–Matrigel solution",
        "temperature": "on ice"
      }
    },
    {
      "id": "s40",
      "inputs": [],
      "notes": "Remove the 12‑well plate containing the first layer and carefully add 0.5 mL of cell‑aggregate‑collagen I–Matrigel solution to each well.",
      "op": "transfer",
      "outputs": [],
      "params": {
        "action": "add 0.5 mL of cell‑aggregate‑collagen I–Matrigel solution to each well of the 12‑well plate",
        "instrument": "e25"
      }
    },
    {
      "id": "s41",
      "inputs": [],
      "notes": "Distribute the aggregates in the wells by rocking the plate back and forth, then move the plate to the incubator.",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "action": "distribute aggregates by rocking plate back and forth"
      }
    },
    {
      "id": "s42",
      "inputs": [],
      "notes": "Incubate the 12‑well plate for 2 h at 37 °C for gel solidification.",
      "op": "incubate",
      "outputs": [],
      "params": {
        "duration": "2 h",
        "temperature": "37 °C"
      }
    },
    {
      "id": "s43",
      "inputs": [],
      "notes": "Add prewarmed (37 °C) StemPro‑34 SFM complete medium including 15% FBS, 100 ng/mL VEGF‑A and 100 ng/mL FGF‑2 to induce blood vessel differentiation.",
      "op": "transfer",
      "outputs": [],
      "params": {
        "action": "add prewarmed StemPro‑34 SFM complete medium with 15% FBS, 100 ng/mL VEGF‑A and 100 ng/mL FGF‑2",
        "instrument": "e32"
      }
    },
    {
      "id": "s44",
      "inputs": [],
      "notes": "Change the medium after 3 d and then every other day.",
      "op": "annotate",
      "outputs": [],
      "params": {}
    },
    {
      "id": "s45",
      "inputs": [],
      "notes": "On day 10, proceed to analysis of blood vessel networks (staining) or to grow/fix vascular organoid cultures.",
      "op": "annotate",
      "outputs": [],
      "params": {}
    },
    {
      "id": "s46",
      "inputs": [],
      "notes": "Fix vascular networks (option A) or grow and fix vascular organoids (option B).",
      "op": "annotate",
      "outputs": [],
      "params": {}
    },
    {
      "id": "s47",
      "inputs": [],
      "notes": "Aspirate the medium from a well using a P1000 pipette.",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "action": "aspirate medium from a well",
        "instrument": "e32"
      }
    },
    {
      "id": "s48",
      "inputs": [
        "e17"
      ],
      "notes": "Add 4% paraformaldehyde (PFA) solution in PBS for 20 min at room temperature.",
      "op": "transfer",
      "outputs": [],
      "params": {
        "duration": "20 min",
        "temperature": "room temperature",
        "volume": "add 4% PFA solution in PBS"
      }
    },
    {
      "id": "s49",
      "inputs": [],
      "notes": "Remove the PFA solution and add PBS.",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "action": "remove PFA solution and add PBS"
      }
    },
    {
      "id": "s50",
      "inputs": [],
      "notes": "Fixed blood vessel networks can be kept in PBS in the refrigerator for up to 1 month (seal with Parafilm).",
      "op": "annotate",
      "outputs": [],
      "params": {}
    },
    {
      "id": "s51",
      "inputs": [],
      "notes": "",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "action": "cut gels into smaller pieces (1/4–1/2 of a 12‑well gel) using extra‑fine scissors and fine forceps",
        "instrument": "e35"
      }
    },
    {
      "id": "s52",
      "inputs": [],
      "notes": "",
      "op": "transfer",
      "outputs": [],
      "params": {
        "action": "transfer gel fragments to a fresh 2‑mL Eppendorf tube",
        "destination": "e26",
        "instrument": "e35"
      }
    },
    {
      "id": "s53",
      "inputs": [],
      "notes": "Option B: liberate the entire matrix from the well bottom.",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "action": "liberate entire matrix from well bottom using sterile spatula",
        "instrument": "e32"
      }
    },
    {
      "id": "s54",
      "inputs": [],
      "notes": "",
      "op": "transfer",
      "outputs": [],
      "params": {
        "action": "transfer gel with sterile lab spoon to lid of a 10‑cm dish",
        "destination": "e28",
        "instrument": "e32"
      }
    },
    {
      "id": "s55",
      "inputs": [],
      "notes": "",
      "op": "manipulate",
      "outputs": [],
      "params": {
        "action": "cut out single blood vessel networks with two sterile 30‑gauge needles",
        "instrument": "e35"
      }
    },
    {
      "id": "s56",
      "inputs": [],
      "notes": "Transfer single organoids to a 6‑well low‑attachment plate and incubate.",
      "op": "transfer",
      "outputs": [],
      "params": {
        "action": "add StemPro‑34 SFM complete medium with 15% FBS, 100 ng/mL VEGF‑A, 100 ng/mL FGF‑2",
        "destination": "e21",
        "instrument": "e32",
        "volume": "5 mL"
      }
    },
    {
      "id": "s57",
      "inputs": [],
      "notes": "One day after separating the blood vessel networks (d11), transfer single organoids to 96‑well plates.",
      "op": "transfer",
      "outputs": [],
      "params": {
        "action": "transfer single organoids to low‑attachment round‑bottom 96‑well ultra‑low‑attachment plates",
        "add_medium": "StemPro‑34 SFM complete medium with 15% FBS, 100 ng/mL VEGF‑A",
        "destination": "e27",
        "instrument": "e32"
      }
    }
  ]
