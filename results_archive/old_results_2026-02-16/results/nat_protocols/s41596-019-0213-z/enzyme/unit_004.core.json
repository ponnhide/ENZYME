      "outputs": [
        "e12"
      ],
      "params": {
        "mode": "confocal imaging",
        "objective": "10×, 20×, or 63×",
        "z_stack": "Nyquist sampling"
      }
    },
    {
      "id": "s13",
      "inputs": [
        "e1",
        "e16"
      ],
      "notes": "Maximum of ~60 organoids per tube.",
