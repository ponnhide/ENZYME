  "steps": [
    {
      "id": "s1",
      "inputs": [
        "e1",
        "e2"
      ],
      "notes": "Transplant blood vessel organoid into the kidney capsule of immunocompromised mouse as previously described.",
      "op": "manipulate",
      "outputs": [
        "e1"
      ],
      "params": {
        "location": "kidney capsule",
        "note": "one organoid per kidney"
      }
    },
    {
      "id": "s2",
      "inputs": [
        "e2"
      ],
      "notes": "If only one kidney was used, keep the opposite kidney as negative control.",
      "op": "manipulate",
      "outputs": [
        "e1"
      ],
      "params": {
        "action": "sacrifice and collect transplanted organoid"
      }
    },
    {
      "id": "s3",
      "inputs": [
        "e1",
        "e3"
      ],
      "notes": "Fix transplants and kidney tissue in 4% PFA solution.",
      "op": "incubate",
      "outputs": [
        "e1"
      ],
      "params": {
        "duration": "overnight",
        "temperature": "4 °C"
      }
    },
    {
      "id": "s4",
      "inputs": [
        "e1"
      ],
      "notes": "Dehydrate and paraffinize samples according to standard histological procedures (details not provided).",
      "op": "annotate",
      "outputs": [
        "e1"
      ],
      "params": {}
    },
    {
      "id": "s5",
      "inputs": [
        "e1",
        "e13"
      ],
      "notes": "Cut sections with a standard cryostat and collect on Ultra Plus slides.",
      "op": "run_device",
      "outputs": [
        "e12"
      ],
      "params": {
        "device": "cryostat",
        "section_thickness": "2-5 μm"
      }
    },
    {
      "id": "s6",
      "inputs": [
        "e12"
      ],
      "notes": "Perform standard de‑paraffinization procedures (details not provided).",
      "op": "annotate",
      "outputs": [
        "e12"
      ],
      "params": {}
    },
    {
      "id": "s7",
      "inputs": [
        "e12"
      ],
      "notes": "Proceed with staining steps 39–45 described below.",
      "op": "annotate",
      "outputs": [
        "e12"
      ],
      "params": {}
    },
    {
      "id": "s8",
      "inputs": [
        "e12"
      ],
      "notes": "Carefully remove all PBS from the slides with a cellulose wipe.",
      "op": "manipulate",
      "outputs": [
        "e12"
      ],
      "params": {
        "action": "remove PBS",
        "tool": "cellulose wipe"
      }
    },
    {
      "id": "s9",
      "inputs": [
        "e12",
        "e9"
      ],
      "notes": "Avoid air bubbles.",
      "op": "manipulate",
      "outputs": [
        "e12"
      ],
      "params": {
        "action": "add mounting medium and lay coverslip",
        "volume": "few drops"
      }
    },
    {
      "id": "s10",
      "inputs": [
        "e12"
      ],
      "notes": "Dry overnight in the dark at room temperature.",
      "op": "incubate",
      "outputs": [
        "e12"
      ],
      "params": {
        "duration": "overnight",
        "light": "dark",
        "temperature": "room temperature"
      }
    },
    {
      "id": "s11",
      "inputs": [
        "e12",
        "e10"
      ],
      "notes": "Seal the slide with nail polish the next day.",
      "op": "manipulate",
      "outputs": [
        "e12"
      ],
      "params": {
        "action": "seal with nail polish"
      }
    },
    {
      "id": "s12",
      "inputs": [
        "e12",
        "e15"
      ],
