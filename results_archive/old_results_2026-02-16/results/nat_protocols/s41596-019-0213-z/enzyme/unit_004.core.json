      "id": "s22",
      "inputs": [
        "e1",
        "e4"
      ],
      "notes": "",
      "op": "wash",
      "outputs": [
        "e1"
      ],
      "params": {
        "duration": "10–15 min",
        "solution": "PBS"
      }
    },
    {
