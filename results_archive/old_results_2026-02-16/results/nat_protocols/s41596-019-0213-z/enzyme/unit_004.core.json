      "outputs": [
        "e1"
      ],
      "params": {
        "action": "rock vertically",
        "device": "orbital shaker",
        "duration": "2 h",
        "temperature": "room temperature",
        "volume": "2 mL per tube"
      }
    },
    {
      "id": "s17",
      "inputs": [
        "e1",
        "e6"
      ],
      "notes": "Primary antibody diluted in blocking buffer.",
      "op": "incubate",
      "outputs": [
        "e1"
      ],
      "params": {
        "action": "rock upright",
        "device": "orbital shaker",
        "duration": "overnight",
        "temperature": "4 °C",
        "volume": "50–100 μL"
      }
    },
    {
      "id": "s18",
      "inputs": [
        "e1",
        "e22"
      ],
