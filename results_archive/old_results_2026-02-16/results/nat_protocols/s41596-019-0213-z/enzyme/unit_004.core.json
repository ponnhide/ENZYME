      }
    },
    {
      "id": "s27",
      "inputs": [
        "e1"
      ],
      "notes": "Dry in the dark overnight.",
      "op": "incubate",
      "outputs": [
        "e1"
      ],
      "params": {
        "duration": "overnight",
        "light": "dark",
        "temperature": "room temperature"
