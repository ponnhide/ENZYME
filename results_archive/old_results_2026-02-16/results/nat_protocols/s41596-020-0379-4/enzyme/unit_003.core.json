  "settings": {
    "biosafety_level": "BSL-2",
    "contains_human_or_animal_material": true,
    "contamination_control": [
      "mycoplasma testing",
      "use of antibiotics in culture medium"
    ],
    "requires_irb_or_iacuc": true,
    "sterility_required": true
  },
  "steps": [
    {
      "id": "s1",
      "inputs": [],
      "notes": "Obtain surgical resections or biopsies from patients (healthy oral mucosa or HNSCC).",
      "op": "allocate",
      "outputs": [
        "e1"
      ],
      "params": {}
    },
    {
      "id": "s2",
      "inputs": [
        "e1"
      ],
      "notes": "Store tissue pieces on ice in basic medium containing 10 µM Y‑27632 (or ice‑cold PBS) to minimize cell death; up to 72 h.",
      "op": "manipulate",
      "outputs": [
        "e1"
      ],
      "params": {
        "additives": [
          "10 µM ROCK inhibitor Y‑27632"
        ],
        "medium": "ice‑cold basic medium",
        "temperature": "4 °C"
      }
    },
    {
      "id": "s3",
      "inputs": [
        "e1"
      ],
      "notes": "Mechanically fragment tissue into ~1 mm pieces.",
      "op": "manipulate",
      "outputs": [
        "e1"
      ],
      "params": {
        "tool": "disposable scalpel, no. 10"
      }
    },
    {
      "id": "s4",
      "inputs": [
        "e1",
        "e3",
        "e4"
      ],
      "notes": "Digest fragments with collagenase (or Liberase) ± DNase to obtain single‑cell suspension or small clumps. Check progress periodically.",
      "op": "incubate",
      "outputs": [
        "e1"
      ],
      "params": {
        "device": "water bath or incubator",
        "duration": "30 min to overnight",
        "temperature": "37 °C"
      }
    },
    {
      "id": "s5",
      "inputs": [
        "e1"
      ],
      "notes": "Filter digested material sequentially through 100 µm then 70 µm strainers.",
      "op": "manipulate",
      "outputs": [
        "e1"
      ],
      "params": {
        "filters": [
          "100 µm cell strainer",
          "70 µm cell strainer"
        ]
      }
    },
    {
      "id": "s6",
      "inputs": [
        "e1"
      ],
      "notes": "Count cells/organoids using a hemocytometer (Glasstic slide).",
      "op": "observe",
      "outputs": [],
      "params": {
        "instrument": "e11",
        "method": "hemocytometer counting"
      }
    },
    {
      "id": "s7",
      "inputs": [
        "e1",
        "e6",
        "e7",
        "e18"
      ],
      "notes": "Mix cell suspension with cold BME/Matrigel (5–10 % final) and dispense organoids into 384‑well plate using Multidrop dispenser.",
      "op": "transfer",
      "outputs": [
        "e15"
      ],
      "params": {
        "ECM_percentage": "5–10% (vol/vol) BME or Matrigel",
        "dispensing_device": "e12",
        "keep_cold": true,
        "plate_type": "384‑well ultra‑low attachment",
        "tubing_prime": "cold PBS"
      }
    },
    {
      "id": "s8",
      "inputs": [
        "e15"
      ],
      "notes": "Culture organoids in humidified incubator; refresh medium every 2–3 days.",
      "op": "incubate",
      "outputs": [
        "e15"
      ],
      "params": {
        "CO₂": "5% (vol/vol)",
        "additive_first_two_passages": "10 µM Y‑27632",
        "device": "e10",
        "duration": "until ready for treatment (typically 1–5 d)",
        "medium_refresh_interval": "2–3 d",
        "temperature": "37 °C"
      }
    },
    {
      "id": "s9",
      "inputs": [
        "e15",
        "e19"
      ],
      "notes": "For passaging, add TrypLE, monitor digestion, stop when appropriate cluster size observed; aid disruption with P1000 pipette.",
      "op": "manipulate",
      "outputs": [
        "e15"
      ],
      "params": {
        "desired_outcome": "clusters of 3–5 cells + single cells",
        "enzyme": "TrypLE",
        "monitor_interval": "5 min",
        "observation_tool": "e11"
      }
    },
    {
      "id": "s10",
      "inputs": [
        "e15"
      ],
      "notes": "If organoids are cystic/grape‑like, use mechanical shearing instead of TrypLE.",
      "op": "manipulate",
      "outputs": [
        "e15"
      ],
      "params": {
        "alternative_method": "mechanical shearing with plugged glass Pasteur pipette"
      }
    },
    {
      "id": "s11",
      "inputs": [
        "e15"
      ],
      "notes": "Add Y‑27632 directly after passaging; not required after medium refresh 2–3 d later.",
      "op": "manipulate",
      "outputs": [
        "e15"
      ],
      "params": {
        "additive": "10 µM ROCK inhibitor Y‑27632"
      }
    },
    {
      "id": "s12",
      "inputs": [],
      "notes": "Prepare cryogenic vials for freezing organoids.",
      "op": "allocate",
      "outputs": [
        "e16"
      ],
      "params": {}
    },
    {
      "id": "s13",
      "inputs": [
        "e15",
        "e16"
      ],
      "notes": "Freeze organoids when highly proliferative; aliquot into cryovials, place in controlled‑rate freezer, then store in liquid nitrogen.",
      "op": "manipulate",
      "outputs": [
        "e16"
      ],
      "params": {
        "container": "e16",
        "freezing_device": "CoolCell LX",
        "freezing_medium": "Recovery cell‑freezing medium",
        "storage": "liquid nitrogen tank",
        "time_post_passage": "2–3 d"
      }
    },
    {
      "id": "s14",
      "inputs": [
        "e16"
      ],
      "notes": "Thaw frozen organoids, wash, and culture; allow one passage before drug screening.",
      "op": "manipulate",
      "outputs": [
        "e15"
      ],
      "params": {
        "post_thaw_culture": "at least one passage before screening",
        "thaw_method": "rapid thaw at 37 °C"
      }
    },
    {
      "id": "s15",
      "inputs": [],
      "notes": "Quality‑control steps (e.g., tumor purity assessment, mycoplasma testing, antibiotic supplementation) are recommended but not detailed here.",
      "op": "annotate",
      "outputs": [],
      "params": {}
    },
    {
      "id": "s16",
      "inputs": [
        "e15",
        "e5"
      ],
      "notes": "Add dispase to wells on day of screen to dissolve ECM.",
      "op": "manipulate",
      "outputs": [
        "e15"
      ],
      "params": {
        "additive": "dispase II",
        "purpose": "remove BME/Matrigel before screening"
      }
    },
    {
      "id": "s17",
      "inputs": [
        "e15"
      ],
      "notes": "Wash organoids, filter to exclude large organoids, then count and adjust density in medium containing 5% BME/Matrigel.",
      "op": "manipulate",
      "outputs": [
        "e15"
      ],
      "params": {
        "filter": "remove large organoids",
        "wash": true
      }
    },
    {
      "id": "s18",
      "inputs": [
        "e15"
      ],
      "notes": "Dispense organoid suspension into 384‑well plate (desired density) using Multidrop dispenser; keep suspension cold to prevent ECM clogging.",
      "op": "transfer",
      "outputs": [
        "e15"
      ],
      "params": {
        "dispensing_device": "e12",
        "keep_cold": true
      }
    },
    {
      "id": "s19",
      "inputs": [
        "e20",
        "e21"
      ],
      "notes": "Add drugs to wells using D300e; include positive and negative controls.",
      "op": "transfer",
      "outputs": [
        "e15"
      ],
      "params": {
        "controls": {
          "negative": "DMSO",
          "positive": "staurosporin (or cycloheximide)"
        },
        "device": "e13",
        "layout": "digital dispenser D300e software layout (Fig. 3b)",
        "replicates": "minimum triplicate"
      }
    },
    {
      "id": "s20",
      "inputs": [
        "e15"
      ],
      "notes": "Incubate plates after drug addition.",
      "op": "incubate",
      "outputs": [
        "e15"
      ],
      "params": {
        "CO₂": "5% (vol/vol)",
        "duration": "5 d (typical) or 1–24 d depending on assay",
        "temperature": "37 °C"
      }
    },
    {
      "id": "s21",
      "inputs": [
        "e8"
      ],
      "notes": "Add CellTiter‑Glo 3D reagent to each well using Multidrop dispenser.",
      "op": "transfer",
      "outputs": [
        "e15"
      ],
      "params": {
        "dispensing_device": "e12",
        "volume_per_well": "as recommended by CellTiter‑Glo 3D protocol"
      }
    },
    {
      "id": "s22",
      "inputs": [
        "e15"
      ],
      "notes": "Allow cell lysis for luminescence development.",
      "op": "incubate",
      "outputs": [
        "e15"
      ],
      "params": {
        "duration": "as per reagent protocol (lysis)",
        "temperature": "room temperature"
      }
    },
    {
      "id": "s23",
      "inputs": [
        "e15"
      ],
      "notes": "Measure luminescence signal from each well.",
      "op": "observe",
      "outputs": [],
      "params": {
        "instrument": "e14",
        "measurement": "luminescence"
      }
    },
    {
      "id": "s24",
      "inputs": [],
      "notes": "Data analysis performed in GraphPad Prism (IC₅₀, AUC, GR metrics). Alternative analysis software may be used.",
      "op": "annotate",
      "outputs": [],
      "params": {}
    }
  ]
}
