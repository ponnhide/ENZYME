  "steps": [
    {
      "id": "s1",
      "inputs": [],
      "notes": "Obtain fresh primary tissue from patient (normal or tumor).",
      "op": "allocate",
      "outputs": [
        "e1"
      ],
      "params": {}
    },
    {
      "id": "s2",
      "inputs": [
        "e1"
      ],
      "notes": "Dissect to remove excess muscle or fat tissue, retaining epithelial tissue.",
      "op": "manipulate",
      "outputs": [
        "e1"
      ],
      "params": {}
    },
    {
      "id": "s3",
      "inputs": [
        "e1"
      ],
      "notes": "Mince tissue into small fragments using a scalpel.",
      "op": "manipulate",
      "outputs": [
        "e1"
      ],
      "params": {
        "fragment_size": "1–3 mm³"
      }
    },
    {
      "id": "s4",
      "inputs": [
        "e1",
        "e2",
        "e6"
      ],
      "notes": "Add trypsin 0.125 % (wt/vol) to the minced tissue in a sterile tube.",
      "op": "transfer",
      "outputs": [
        "e6"
      ],
      "params": {
        "reagent_volume": "sufficient to cover tissue"
      }
    },
    {
      "id": "s5",
      "inputs": [
        "e6"
      ],
      "notes": "Digest tissue, monitoring closely; resuspend the suspension every 10 min. Do not exceed 60 min.",
      "op": "incubate",
      "outputs": [
        "e6"
      ],
      "params": {
        "max_time": "60 min",
        "resuspend_interval": "10 min",
        "temperature": "37 °C"
      }
    },
    {
      "id": "s6",
      "inputs": [
        "e6",
        "e5"
      ],
      "notes": "Filter the digestion suspension through a 100 µm‑pore cell strainer to remove debris.",
      "op": "manipulate",
      "outputs": [
        "e6"
      ],
      "params": {}
    },
    {
      "id": "s7",
      "inputs": [
        "e6"
      ],
      "notes": "Pellet cells by centrifugation.",
      "op": "centrifuge",
      "outputs": [
        "e6"
      ],
      "params": {
        "note": "Centrifuge to increase yield (speed and time not specified).",
        "purpose": "pellet cells"
      }
    },
    {
      "id": "s8",
      "inputs": [
        "e6"
      ],
      "notes": "Remove supernatant, keep the cell pellet on ice (cold).",
      "op": "manipulate",
      "outputs": [
        "e6"
      ],
      "params": {}
    },
    {
      "id": "s9",
      "inputs": [
        "e6",
        "e3"
      ],
      "notes": "Resuspend the cell pellet in basement membrane extract (BME).",
      "op": "transfer",
      "outputs": [
        "e6"
      ],
      "params": {
        "reagent_volume": "sufficient to resuspend pellet"
      }
    },
    {
      "id": "s10",
      "inputs": [
        "e6",
        "e7"
      ],
      "notes": "Plate the BME‑cell mixture onto a pre‑warmed 384‑well plate; invert the plate to allow the BME to solidify.",
      "op": "manipulate",
      "outputs": [
        "e7"
      ],
      "params": {}
    },
    {
      "id": "s11",
      "inputs": [
        "e7"
      ],
      "notes": "Culture organoids in the incubator with appropriate organoid culture medium.",
      "op": "incubate",
      "outputs": [
        "e7"
      ],
      "params": {
        "CO2": "5 %",
        "duration": "until organoids form (typically 7–12 days)",
        "temperature": "37 °C"
      }
    },
    {
      "id": "s12",
      "inputs": [],
      "notes": "Further steps such as drug screening, passage, cryopreservation, and detailed medium composition are described in the paper but not specified in sufficient detail for automated translation.",
      "op": "annotate",
      "outputs": [],
      "params": {}
    }
  ]
