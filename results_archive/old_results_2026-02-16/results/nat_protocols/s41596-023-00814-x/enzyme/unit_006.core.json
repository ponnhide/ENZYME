      ],
      "params": {
        "method": "remove medium carefully with a 200 µl micropipette tip without disturbing organoids",
        "volume": "carried‑over medium (unspecified)"
      }
    },
    {
      "id": "s3",
      "inputs": [
        "e2",
        "e5"
      ],
      "notes": "Wash organoids three times with 3 ml PBS and withdraw the PBS after each wash.",
      "op": "annotate",
      "outputs": [
