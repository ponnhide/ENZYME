      "params": {
        "description": "Add 2 ml Accutase containing 0.2 U/µl DNase I (pre‑warmed in a 37 °C water bath). Close tube tightly.",
        "volume": "2 ml"
      }
    },
    {
      "id": "s5",
      "inputs": [
        "e2"
      ],
      "notes": "After 15 min, check dissociation by shaking the tube and observing organoid integrity. Then dissociate by pipetting up and down with an uncut 1,000 µl micropipette tip.",
      "op": "incubate",
      "outputs": [
        "e2"
      ],
      "params": {
        "device": "e8",
        "duration": "15 min",
        "temperature": "37 °C"
      }
    },
