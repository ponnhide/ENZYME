      "id": "s6",
      "inputs": [
        "e2"
      ],
      "notes": "",
      "op": "incubate",
      "outputs": [
        "e2"
      ],
      "params": {
        "device": "e8",
        "duration": "15 min",
        "temperature": "37 °C"
      }
    },
    {
