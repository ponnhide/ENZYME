    {
      "id": "s16",
      "op": "incubate",
      "inputs": [
        "e_plate_embedded"
      ],
      "outputs": [
        "e_plate_embedded"
      ],
      "params": {
        "incubator_id": "e_incubator",
        "temperature_C": 37,
        "CO2_percent": 5,
        "duration_d": 2
      },
      "notes": "Culture embedded aggregates for 2 days (d‑2 to d0) in RB‑ medium without insulin."
    },
    {
      "id": "s17",
      "op": "manipulate",
      "inputs": [
        "e_chir_working",
        "e_plate_embedded"
      ],
      "outputs": [
        "e_plate_chir"
      ],
      "params": {
        "action": "add_chir",
        "final_concentration_uM": 7.5,
        "duration_h": 24
      },
      "notes": "On day 0, add CHIR99021 working solution to each well to reach 7.5 µM final concentration; culture for 24 h."
    },
    {
      "id": "s18",
      "op": "incubate",
      "inputs": [
        "e_plate_chir"
      ],
      "outputs": [
        "e_plate_chir"
      ],
      "params": {
        "incubator_id": "e_incubator",
        "temperature_C": 37,
        "CO2_percent": 5,
        "duration_h": 24
      },
      "notes": "Incubate with CHIR for 24 h (d0–d1)."
    },
    {
      "id": "s19",
      "op": "manipulate",
      "inputs": [
        "e_plate_chir"
      ],
      "outputs": [
        "e_plate_post_chir"
