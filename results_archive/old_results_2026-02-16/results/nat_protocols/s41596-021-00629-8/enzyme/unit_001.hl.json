      "outputs": [],
      "params": {
        "instrument_id": "e_inverted_microscope",
        "objective": "low magnification",
        "purpose": "verify proper patterning of HFOs"
      },
      "notes": "Examine HFOs under an inverted microscope to ensure correct morphology before downstream analyses."
    },
    {
      "id": "s25",
      "op": "annotate",
      "inputs": [],
      "outputs": [],
      "params": {},
