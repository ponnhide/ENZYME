          },
          "from": {
            "id": "c1",
            "kind": "container"
          },
          "to": {
            "id": "c1",
            "kind": "container"
          }
        }
      },
      {
        "annotations": {
          "note": "Aspirate EDTA, add 0.6 mL Accutase per well."
        },
        "id": "s2",
        "inputs": [
          {
            "id": "s2",
            "kind": "sample"
          }
        ],
        "op": "transfer",
        "outputs": [
