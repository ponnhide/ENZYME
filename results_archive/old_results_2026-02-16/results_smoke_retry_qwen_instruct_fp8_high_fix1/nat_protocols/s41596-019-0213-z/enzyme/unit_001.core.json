    "steps": [
      {
        "annotations": {
          "note": "Remove E8 medium and replace with 0.6 mL of 0.5 mM EDTA per well of a 6-well plate."
        },
        "id": "s1",
        "inputs": [
          {
            "id": "s1",
            "kind": "sample"
          }
        ],
