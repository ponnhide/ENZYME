          "program": {
            "duration": {
              "unit": "d",
              "value": 1
            }
          }
        }
      },
      {
        "annotations": {
          "note": "Transfer aggregates to 15-mL Falcon tube using 10-mL serological pipette; wait for gravitation settling. Do not discard 6-well plate."
        },
        "id": "s12",
        "inputs": [
          {
            "id": "s10",
            "kind": "sample"
          }
        ],
        "op": "transfer",
        "outputs": [
