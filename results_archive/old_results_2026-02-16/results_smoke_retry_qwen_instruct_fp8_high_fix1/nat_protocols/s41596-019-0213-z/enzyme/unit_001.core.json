          }
        ],
        "params": {
          "amount": {
            "unit": "mL",
            "value": 3
          },
          "from": {
            "id": "c2",
            "kind": "container"
          },
          "to": {
            "id": "c3",
            "kind": "container"
          }
        }
      },
      {
        "annotations": {
          "note": "Incubate for 2 more days."
        },
        "id": "s17",
        "inputs": [
          {
            "id": "s15",
            "kind": "sample"
          }
