        "to": "s4"
      },
      {
        "from": "s4",
        "to": "s5"
      },
      {
        "from": "s5",
        "to": "s6"
      },
      {
        "from": "s6",
        "to": "s7"
      },
      {
        "from": "s7",
        "to": "s8"
      },
      {
        "from": "s8",
        "to": "s9"
      },
      {
        "from": "s9",
        "to": "s10"
      },
      {
        "from": "s10",
        "to": "s11"
      }
    ],
    "detail_level": 0
  }
}