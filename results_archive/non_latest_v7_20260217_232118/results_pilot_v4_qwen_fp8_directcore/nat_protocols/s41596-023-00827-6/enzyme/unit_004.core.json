  "protocol": {
    "steps": [
      {
        "id": "s1",
        "op": "manipulate",
        "params": {
          "action_kind": "dislodge_cells"
        },
        "inputs": [
          {
            "kind": "sample",
            "id": "24-well transwell insert"
          }
        ],
        "outputs": [],
        "annotations": [
          "Remove the insert from the 24-well plate, invert it and gently peel off the membrane from the bottom of the insert using forceps."
        ]
      },
      {
        "id": "s2",
        "op": "transfer",
        "params": {
          "amount": "1 ml"
        },
        "inputs": [
          {
            "kind": "sample",
            "id": "24-well transwell insert"
          }
        ],
        "outputs": [
          {
            "kind": "sample",
            "id": "15 ml centrifuge tube"
          }
        ],
        "annotations": [
          "Carefully place the insert with the membrane removed over the mouth of an open, empty 15 ml centrifuge tube."
        ]
      },
      {
        "id": "s3",
        "op": "transfer",
        "params": {
          "amount": "1 ml"
        },
        "inputs": [
          {
            "kind": "sample",
            "id": "cell recovery reagent"
          }
        ],
        "outputs": [
          {
            "kind": "sample",
            "id": "15 ml centrifuge tube"
          }
        ],
        "annotations": [
          "Add 1 ml of cell recovery reagent to the insert such that the embedded organoid with Matrigel falls into the 15 ml centrifuge tube."
        ]
      },
      {
        "id": "s4",
        "op": "transfer",
        "params": {
          "amount": "1 ml"
        },
        "inputs": [
          {
            "kind": "sample",
            "id": "15 ml centrifuge tube"
          }
        ],
        "outputs": [],
        "annotations": [
          "Close the 15 ml centrifuge tube and immediately place on ice to depolymerize the Matrigel."
        ]
      },
      {
        "id": "s5",
        "op": "manipulate",
        "params": {
          "action_kind": "mix_gently"
        },
        "inputs": [
          {
            "kind": "sample",
            "id": "15 ml centrifuge tube"
          }
        ],
        "outputs": [],
        "annotations": [
          "Every 20 min, gently shake the tube to check if the Matrigel has dissolved and place it back on ice."
        ]
      },
      {
        "id": "s6",
        "op": "transfer",
        "params": {
          "amount": "10 ml"
        },
        "inputs": [
          {
            "kind": "sample",
            "id": "wash medium"
          }
        ],
        "outputs": [
          {
            "kind": "sample",
            "id": "15 ml centrifuge tube"
          }
        ],
        "annotations": [
          "Once the Matrigel has dissolved, add 10 ml of wash medium to the tube. Time varies from ~45 min to 1 h depending on Matrigel thickness."
        ]
      },
      {
        "id": "s7",
        "op": "run_device",
        "params": {
          "device_kind": "centrifuge",
          "program": {
            "speed": "200g",
            "duration": "5 min"
          }
        },
        "inputs": [
          {
            "kind": "sample",
            "id": "15 ml centrifuge tube"
          }
        ],
        "outputs": [
          {
            "kind": "sample",
            "id": "15 ml centrifuge tube"
          }
        ]
      },
      {
        "id": "s8",
        "op": "transfer",
        "params": {
          "amount": "1 ml"
        },
        "inputs": [
          {
            "kind": "sample",
            "id": "15 ml centrifuge tube"
          }
        ],
        "outputs": [],
        "annotations": [
          "Aspirate the wash medium and cell recovery reagent and add 1 ml of Accutase to the organoid and incubate at 37 °C."
        ]
      },
      {
        "id": "s9",
        "op": "manipulate",
        "params": {
          "action_kind": "mix_gently"
        },
        "inputs": [
          {
            "kind": "sample",
            "id": "15 ml centrifuge tube"
          }
        ],
        "outputs": [],
        "annotations": [
          "Gently shake the tube to disturb/dissociate the organoid every 5 min and replace the tube at 37 °C for 10–12 min. Time varies depending on the organoid size and density."
        ]
      },
      {
        "id": "s10",
        "op": "transfer",
        "params": {
          "amount": "10 ml"
        },
        "inputs": [
          {
            "kind": "sample",
            "id": "wash medium"
          }
        ],
        "outputs": [
          {
            "kind": "sample",
            "id": "15 ml centrifuge tube"
          }
        ],
        "annotations": [
          "When the organoid is dissociated add 10 ml wash medium and centrifuge at 400g for 4 min."
        ]
      },
      {
        "id": "s11",
        "op": "run_device",
        "params": {
          "device_kind": "centrifuge",
          "program": {
            "speed": "400g",
            "duration": "4 min"
          }
        },
        "inputs": [
          {
            "kind": "sample",
            "id": "15 ml centrifuge tube"
          }
        ],
        "outputs": [
          {
            "kind": "sample",
            "id": "15 ml centrifuge tube"
          }
        ]
      },
      {
        "id": "s12",
        "op": "transfer",
        "params": {
          "amount": "1 ml"
        },
        "inputs": [
          {
            "kind": "sample",
            "id": "15 ml centrifuge tube"
          }
        ],
        "outputs": [],
        "annotations": [
          "Aspirate the wash medium and resuspend the pellet in base medium."
        ]
      },
      {
        "id": "s13",
        "op": "annotate",
        "params": {},
        "inputs": [],
        "outputs": [],
        "annotations": [
          "CRITICAL STEP Check the base medium compatibility with the sequencing technology to be used for scRNAseq."
        ]
      },
      {
        "id": "s14",
        "op": "run_device",
        "params": {
          "device_kind": "dna_delivery_device",
          "program": {
            "kit": "MiltenyiBiotec dead cell removal kit"
          }
        },
        "inputs": [
          {
            "kind": "sample",
            "id": "15 ml centrifuge tube"
          }
        ],
        "outputs": [
          {
            "kind": "sample",
            "id": "15 ml centrifuge tube"
          }
        ],
        "annotations": [
          "Remove dead cells from the cell suspension by using the MiltenyiBiotec dead cell removal kit."
        ]
      },
      {
        "id": "s15",
        "op": "annotate",
        "params": {},
        "inputs": [],
        "outputs": [],
        "annotations": [
          "CRITICAL STEP Cell suspension dilution varies depending on the sequencing vendor requirements."
        ]
      },
      {
        "id": "s16",
        "op": "manipulate",
        "params": {
          "action_kind": "label"
        },
        "inputs": [
          {
            "kind": "sample",
            "id": "15 ml centrifuge tube"
          }
        ],
        "outputs": [],
        "annotations": [
          "Dilute the cell suspension to a concentration of 5,000 cells/μl for sequencing."
        ]
      },
      {
        "id": "s17",
        "op": "annotate",
        "params": {},
        "inputs": [],
        "outputs": [],
        "annotations": [
          "For sequencing and analysis of scRNAseq data, see Box 3."
        ]
      }
    ],
