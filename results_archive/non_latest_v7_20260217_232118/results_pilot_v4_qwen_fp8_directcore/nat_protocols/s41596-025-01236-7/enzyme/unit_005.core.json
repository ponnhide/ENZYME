    "steps": [
      {
        "id": "s1",
        "op": "annotate",
        "params": {
          "note": "Cultrex (R&D Systems) is used as a Matrigel alternative; no differences observed in supporting hPSC expansion."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s2",
        "op": "manipulate",
        "params": {
          "action_kind": "resuspend"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s3",
        "op": "manipulate",
        "params": {
          "action_kind": "resuspend"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s4",
        "op": "annotate",
        "params": {
          "note": "hPSCs thawed for experimental use should recover for two to three passages before differentiation."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s5",
        "op": "annotate",
        "params": {
          "note": "Use hPSC lines with no more than 75 passages."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s6",
        "op": "annotate",
        "params": {
          "note": "Differentiation of hPSCs to iNPCs is optimized based on previous studies; reproducible in H1 and H9 hESC lines."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s7",
        "op": "run_device",
        "params": {
          "device_kind": "incubator",
          "program": "Stage I medium for differentiation to late primitive streak"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s8",
        "op": "run_device",
        "params": {
          "device_kind": "incubator",
          "program": "Stage II medium for specification of posterior intermediate mesoderm"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s9",
        "op": "run_device",
        "params": {
          "device_kind": "incubator",
          "program": "Stage III medium with high FGF9 to enrich NPC populations"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s10",
        "op": "manipulate",
        "params": {
          "action_kind": "resuspend"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s11",
        "op": "observe",
        "params": {
          "modality": "instrument_readout",
          "features": [
            "SIX2-GFP",
            "PAX2-mCherry"
          ]
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s12",
        "op": "run_device",
        "params": {
          "device_kind": "cell_counter",
          "program": "FACS sorting for SIX2-GFP and PAX2-mCherry double-positive cells"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s13",
        "op": "annotate",
        "params": {
          "note": "For hPSC lines without reporter system, culture differentiated cells in hNPSR-v2 medium for two to three passages to enrich iNPCs (~90% SIX2 positivity by immunostaining)."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s14",
        "op": "run_device",
        "params": {
          "device_kind": "cell_counter",
          "program": "FACS sorting for ITGA8 to purify off-target cells"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s15",
        "op": "transfer",
        "params": {
          "from_container": "c3",
          "to_container": "c3",
          "amount": {
            "value": 1.0,
            "unit": "mL"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s16",
        "op": "run_device",
        "params": {
          "device_kind": "incubator",
          "program": "Culture iNPCs in hNPSR-v2 medium"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s17",
        "op": "annotate",
        "params": {
