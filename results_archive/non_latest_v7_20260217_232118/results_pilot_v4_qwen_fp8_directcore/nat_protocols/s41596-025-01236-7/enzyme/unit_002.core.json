  "protocol": {
    "steps": [
      {
        "id": "s1",
        "op": "annotate",
        "params": {
          "note": "During stage two (D4–D7) to generate posterior intermediate mesoderm from primitive streak, cell death was observed in original protocol. Y-27632 was added to improve survival."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s2",
        "op": "transfer",
        "params": {
          "source": "m1",
          "target": "culture_medium",
          "notes": "Y-27632 added to stage two medium",
          "amount": "10.0 µL per mL"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s3",
        "op": "transfer",
        "params": {
          "source": "m2",
          "target": "culture_medium",
          "notes": "Activin A retained from original protocol",
          "amount": "1.0 µL per mL"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s4",
        "op": "transfer",
        "params": {
          "source": "m3",
          "target": "culture_medium",
          "notes": "FGF9 at 50 ng/mL used in stage three instead of 10 ng/mL",
          "amount": "1.0 µL per mL"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s5",
        "op": "observe",
        "params": {
          "modality": "instrument_readout",
          "features": [
            "SIX2+ cell percentage",
            "SIX2+/PAX2+ cell percentage"
          ]
        },
        "notes": "FACS analysis at end of stage three shows 30–50% SIX2+, 10–20% SIX2+/PAX2+",
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s6",
        "op": "transfer",
        "params": {
          "source": "samples",
          "target": "c1",
          "notes": "FACS-purified SIX2+/PAX2+ iNPCs transferred to hNPSR-v2 medium",
          "amount": {
            "value": 100.0,
            "unit": "µL"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s7",
        "op": "run_device",
        "params": {
          "device_kind": "incubator",
          "program": "37°C, 5% CO2"
        },
        "notes": "iNPCs cultured in hNPSR-v2 medium under standard conditions",
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s8",
        "op": "annotate",
        "params": {
          "note": "hNPSR-v2 medium derived from mNPSR-v2 by replacing mouse LIF with human LIF; supplemented with TRULI to enhance YAP activity."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s9",
        "op": "transfer",
        "params": {
          "source": "m7",
          "target": "medium",
          "notes": "Human LIF substituted for mouse LIF in hNPSR-v1",
          "amount": {
            "value": 10.0,
            "unit": "ng/mL"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s10",
        "op": "transfer",
        "params": {
          "source": "m14",
          "target": "medium",
          "notes": "TRULI added to hNPSR-v1 to form hNPSR-v2",
          "amount": "1.0 µL per mL"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s11",
        "op": "run_device",
        "params": {
          "device_kind": "incubator",
