  "protocol": {
    "steps": [
      {
        "id": "s1",
        "op": "annotate",
        "inputs": [],
        "outputs": [],
        "params": {},
        "annotations": {
          "note": "During stage two (D4–D7) to generate posterior intermediate mesoderm from primitive streak, cell death was observed with original protocol; Y-27632 (Rock inhibitor) was added to improve survival, in addition to activin A."
        }
      },
      {
        "id": "s2",
        "op": "annotate",
        "inputs": [],
        "outputs": [],
        "params": {},
        "annotations": {
          "note": "In stage three, FGF9 concentration was increased from 10 ng ml⁻¹ to 50 ng ml⁻¹ to increase SIX2+/PAX2+ iNPC induction efficiency based on dual reporter expression."
        }
      },
      {
        "id": "s3",
        "op": "annotate",
        "inputs": [],
        "outputs": [],
        "params": {},
        "annotations": {
          "note": "At end of stage three, 30–50% of cells are SIX2+ (10–20% SIX2+/PAX2+) by FACS analysis."
        }
      },
      {
        "id": "s4",
        "op": "observe",
        "inputs": [
          {
            "kind": "sample",
            "id": "s6"
          }
        ],
        "outputs": [],
        "params": {
          "modality": "instrument_readout",
          "features": [
            "SIX2 expression",
            "PAX2 expression"
          ]
        },
        "annotations": {}
      },
      {
        "id": "s5",
        "op": "transfer",
        "inputs": [
          {
            "kind": "sample",
            "id": "s6"
          }
        ],
        "outputs": [
          {
            "kind": "sample",
            "id": "s7"
          }
        ],
        "params": {
          "amount": {
            "value": 1,
            "unit": "fraction"
          }
        },
        "annotations": {
          "note": "FACS-purified SIX2+/PAX2+ iNPCs were transferred to hNPSR-v2 medium for expansion."
        }
      },
      {
        "id": "s6",
        "op": "annotate",
        "inputs": [],
        "outputs": [],
        "params": {},
        "annotations": {
          "note": "hNPSR-v2 medium was developed from mNPSR-v2, which was optimized from mNPSR-v1 for 2D monolayer culture of mouse NPCs; key additions: SB202190 (p38 MAPK inhibitor) and DAPT (Notch inhibitor); CHIR99021 concentration increased from 1 µM to 1.5 µM."
        }
      },
      {
        "id": "s7",
        "op": "annotate",
        "inputs": [],
        "outputs": [],
        "params": {},
        "annotations": {
          "note": "hNPSR-v1 was formulated by replacing mouse LIF with human LIF; human iNPCs maintained NPC identity for only 2 weeks before decline in proliferation and marker expression."
        }
      },
      {
        "id": "s8",
        "op": "annotate",
        "inputs": [],
        "outputs": [],
        "params": {},
        "annotations": {
          "note": "Decreased YAP activity observed in iNPCs cultured in hNPSR-v1; TRULI (YAP agonist) was supplemented to formulate hNPSR-v2 medium."
        }
      },
      {
        "id": "s9",
        "op": "annotate",
        "inputs": [],
        "outputs": [],
        "params": {},
        "annotations": {
          "note": "hNPSR-v2 medium enables long-term expansion of iNPCs for months with retained NPC marker expression and nephrogenic potential; allows enrichment of transient iNPCs without prior FACS purification or genome editing."
        }
      },
      {
        "id": "s10",
        "op": "manipulate",
        "inputs": [
          {
            "kind": "sample",
            "id": "s7"
          }
        ],
        "outputs": [
          {
            "kind": "sample",
            "id": "s8"
          }
        ],
        "params": {
          "action_kind": "dislodge_cells"
        },
        "annotations": {
          "note": "Long-term expanded iNPCs in hNPSR-v2 medium were dissociated into single cells."
        }
      },
      {
        "id": "s11",
        "op": "transfer",
        "inputs": [
          {
            "kind": "sample",
            "id": "s8"
          }
        ],
        "outputs": [
          {
            "kind": "sample",
            "id": "s9"
          }
        ],
        "params": {
          "amount": {
            "value": 1,
            "unit": "fraction"
          }
        },
        "annotations": {
          "note": "Single iNPCs were transferred to 96-well low-attachment U-bottom plates to form aggregates."
        }
      },
      {
        "id": "s12",
        "op": "run_device",
        "inputs": [
          {
            "kind": "sample",
            "id": "s9"
          }
        ],
        "outputs": [
          {
            "kind": "sample",
            "id": "s10"
          }
        ],
        "params": {
          "device_kind": "incubator",
          "program": "air-liquid interface culture in Transwell system"
        },
        "annotations": {
          "note": "iNPC aggregates were transferred to Transwell culture at air–liquid interface to mimic mouse kidney explant culture conditions."
        }
      },
      {
        "id": "s13",
        "op": "transfer",
        "inputs": [
          {
            "kind": "sample",
            "id": "s10"
          }
        ],
        "outputs": [
          {
            "kind": "sample",
            "id": "s11"
          }
        ],
        "params": {
          "amount": {
            "value": 1,
            "unit": "fraction"
          }
        },
        "annotations": {
          "note": "CHIR99021 was added to mimic WNT9b signal for mesenchymal-to-epithelial transition (MET) and nephron induction."
        }
      },
      {
        "id": "s14",
        "op": "transfer",
        "inputs": [
          {
            "kind": "sample",
            "id": "s11"
          }
        ],
        "outputs": [
          {
            "kind": "sample",
            "id": "s12"
          }
        ],
        "params": {
          "amount": {
            "value": 1,
            "unit": "fraction"
          }
        },
        "annotations": {
          "note": "On day 5, medium was changed from APEL2 basal to advanced RPMI 1640 to support organoid structural integrity."
        }
      },
      {
        "id": "s15",
        "op": "transfer",
        "inputs": [
          {
            "kind": "sample",
            "id": "s12"
          }
        ],
        "outputs": [
          {
            "kind": "sample",
            "id": "s13"
          }
        ],
        "params": {
          "amount": {
            "value": 1,
            "unit": "fraction"
          }
        },
        "annotations": {
          "note": "A83-01 (TGF-β inhibitor) was supplemented to enhance organoid maturity, as observed by collagen markers in podocytes and megalin in proximal tubules."
        }
      },
      {
        "id": "s16",
        "op": "annotate",
        "inputs": [],
        "outputs": [],
        "params": {},
        "annotations": {
          "note": "Major nephron segments are induced within 2 weeks of iNPC differentiation; increased maturity observed after 3 weeks."
        }
      }
