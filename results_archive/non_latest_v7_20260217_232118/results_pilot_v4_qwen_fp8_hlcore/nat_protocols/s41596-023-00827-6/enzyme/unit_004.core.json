    "start_step_id": "s1",
    "step_order": null,
    "steps": [
      {
        "annotations": {
          "note": "Remove the insert from the 24-well plate, invert it and gently peel off the membrane from the bottom of the insert using forceps."
        },
        "id": "s1",
        "inputs": [
          {
            "id": "c2",
            "kind": "container"
          }
        ],
        "op": "annotate",
        "outputs": [
          {
            "id": "c2",
            "kind": "container"
          }
        ],
        "params": {
          "text": "unmapped action_kind in source: peel"
        }
      },
      {
        "annotations": {
          "note": "Carefully place the insert with the membrane removed over the mouth of an open, empty 15 ml centrifuge tube."
        },
        "id": "s2",
        "inputs": [
          {
            "id": "c2",
            "kind": "container"
          }
        ],
        "op": "annotate",
        "outputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "params": {
          "text": "unmapped action_kind in source: place_over"
        }
      },
      {
        "annotations": {
          "note": "Add 1 ml of cell recovery reagent to the insert such that the embedded organoid with Matrigel falls into the 15 ml centrifuge tube."
        },
        "id": "s3",
        "inputs": [
          {
            "id": "m1",
            "kind": "material"
          }
        ],
        "op": "transfer",
        "outputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "params": {
          "amount": {
            "unit": "ml",
            "value": 1
          }
        }
      },
      {
        "annotations": {
          "note": "Close the 15 ml centrifuge tube and immediately place on ice to depolymerize the Matrigel."
        },
        "id": "s4",
        "inputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "op": "annotate",
        "outputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "params": {
          "text": "unmapped action_kind in source: close_and_place_on_ice"
        }
      },
      {
        "annotations": {
          "note": "Every 20 min, gently shake the tube to check if the Matrigel has dissolved and place it back on ice."
        },
        "id": "s5",
        "inputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "op": "annotate",
        "outputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "params": {
          "text": "unmapped action_kind in source: shake_gently"
        }
      },
      {
        "annotations": {
          "note": "Once the Matrigel has dissolved, add 10 ml of wash medium to the tube. Time varies from ~45 min to 1 h depending on Matrigel thickness."
        },
        "id": "s6",
        "inputs": [
          {
            "id": "m2",
            "kind": "material"
          }
        ],
        "op": "transfer",
        "outputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "params": {
          "amount": {
            "unit": "ml",
            "value": 10
          }
        }
      },
      {
        "id": "s7",
        "inputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "op": "run_device",
        "outputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "params": {
          "device_kind": "centrifuge",
          "program": {
            "duration": "5 min",
            "speed": "200g"
          }
        }
      },
      {
        "annotations": {
          "note": "Aspirate the wash medium and cell recovery reagent and add 1 ml of Accutase to the organoid and incubate at 37 °C. CRITICAL STEP Avoid using trypsin to dissociate the organoid as it is too harsh and the chances of losing fragile cell types is higher."
        },
        "id": "s8",
        "inputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "op": "transfer",
        "outputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "params": {
          "amount": {
            "unit": "ml",
            "value": 1
          }
        }
      },
      {
        "annotations": {
          "note": "Gently shake the tube to disturb/dissociate the organoid every 5 min and replace the tube at 37 °C for 10–12 min. Time varies depending on the organoid size and density."
        },
        "id": "s9",
        "inputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "op": "annotate",
        "outputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "params": {
          "text": "unmapped action_kind in source: shake_gently"
        }
      },
      {
        "annotations": {
          "note": "When the organoid is dissociated add 10 ml wash medium and centrifuge at 400g for 4 min."
        },
        "id": "s10",
        "inputs": [
          {
            "id": "m2",
            "kind": "material"
          }
        ],
        "op": "transfer",
        "outputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "params": {
          "amount": {
            "unit": "ml",
            "value": 10
          }
        }
      },
      {
        "id": "s11",
        "inputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "op": "run_device",
        "outputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "params": {
          "device_kind": "centrifuge",
          "program": {
            "duration": "4 min",
            "speed": "400g"
          }
        }
      },
      {
        "annotations": {
          "note": "Aspirate the wash medium and resuspend the pellet in base medium. CRITICAL STEP Check the base medium compatibility with the sequencing technology to be used for scRNAseq."
        },
        "id": "s12",
        "inputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "op": "transfer",
        "outputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "params": {
          "amount": {
            "unit": "ml",
            "value": 1
          }
        }
      },
      {
        "annotations": {
          "note": "Remove dead cells from the cell suspension by using the MiltenyiBiotec dead cell removal kit."
        },
        "id": "s13",
        "inputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "op": "annotate",
        "outputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "params": {
          "text": "unmapped action_kind in source: use_kit"
        }
      },
      {
        "annotations": {
          "note": "Dilute the cell suspension to a concentration of 5,000 cells/μl for sequencing. For sequencing and analysis of scRNAseq data, see Box 3. CRITICAL STEP Cell suspension dilution varies depending on the sequencing vendor requirements."
        },
        "id": "s14",
        "inputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "op": "transfer",
        "outputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "params": {
          "amount": {
            "unit": "cells/μl",
            "value": 5000
          }
        }
      }
    ]
