      {
        "annotations": {
          "text": "Cut an ~40 mm square of Parafilm and disinfect it by spraying with 70% ethanol."
        },
        "id": "s2",
        "inputs": [
          {
            "id": "s_parafilm",
            "kind": "sample"
          }
        ],
        "op": "annotate",
        "outputs": [],
        "params": {}
      },
      {
        "annotations": {},
