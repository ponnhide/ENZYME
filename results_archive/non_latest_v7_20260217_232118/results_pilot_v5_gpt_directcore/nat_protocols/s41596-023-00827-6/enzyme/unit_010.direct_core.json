        "id": "s2",
        "op": "annotate",
        "params": {
          "note": "Remove the insert from the 24‑well plate, invert it and gently peel off the membrane from the bottom of the insert using forceps."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s3",
        "op": "transfer",
        "params": {
          "source_container": "c2",
          "dest_container": "c3",
          "description": "Place the insert with the membrane removed over the mouth of the empty 15 ml centrifuge tube.",
          "amount": {
            "value": 0,
            "unit": "ml"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s4",
        "op": "transfer",
        "params": {
          "source_material": "m1",
          "dest_container": "c3",
          "description": "Add 1 ml of cell recovery reagent to the insert so that the embedded organoid with Matrigel falls into the tube.",
          "amount": {
            "value": 1.0,
            "unit": "ml"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s5",
        "op": "annotate",
        "params": {
          "note": "Close the 15 ml centrifuge tube and immediately place on ice to depolymerize the Matrigel."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s6",
        "op": "annotate",
        "params": {
          "note": "Every 20 min, gently shake the tube to check if the Matrigel has dissolved and place it back on ice."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s7",
        "op": "transfer",
        "params": {
          "source_material": "m2",
          "dest_container": "c3",
          "description": "Add 10 ml of wash medium to the tube after Matrigel dissolution.",
          "amount": {
            "value": 10.0,
            "unit": "ml"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s8",
        "op": "run_device",
