          "device_kind": "centrifuge",
          "device_ref": "e1",
          "program": {
            "program_name": "200g_5min"
          }
        },
        "outputs": [],
        "inputs": []
      },
      {
        "id": "s9",
        "op": "annotate",
        "params": {
          "note": "Aspirate the wash medium and cell recovery reagent."
        },
        "inputs": [],
        "outputs": []
      },
