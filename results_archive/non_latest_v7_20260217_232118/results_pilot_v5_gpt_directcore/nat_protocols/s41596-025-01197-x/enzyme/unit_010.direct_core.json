      },
      {
        "id": "s11",
        "op": "allocate",
        "params": {
          "allocate_kind": "sample",
          "sample_name": "MG aliquot"
        },
        "outputs": [],
        "inputs": []
      },
      {
        "id": "s12",
        "op": "annotate",
        "params": {
          "note": "Place a box of P200 pipette tips in a −20 °C freezer for at least 15 min to cool them before use for embedding."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s13",
        "op": "annotate",
        "params": {
          "note": "Thaw MG aliquot(s) on ice 1–2 h before embedding; 30 µL of MG will be needed for embedding each organoid."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s14",
        "op": "annotate",
        "params": {
          "note": "Cut an ~40 mm square of Parafilm and disinfect it by spraying with 70% ethanol. Place the Parafilm over the plastic portion of an empty P200 pipette tip holder. Using the rubber end of a 3 mL syringe plunger, apply pressure to the parafilm directly above a hole to form a dimple. Repeat to make the necessary number of dimples."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s15",
        "op": "annotate",
        "params": {
          "note": "Carefully detach the Parafilm square from the pipette tip holder by peeling it away while maintaining the dimple shape."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s16",
        "op": "annotate",
        "params": {
          "note": "Place the dimpled Parafilm square on the inside of a sterile 10 cm Petri dish lid and press the Parafilm corners onto the surface using a fingernail so that it becomes stuck to the lid."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s17",
        "op": "transfer",
        "params": {
          "source": "organoid_sample",
          "destination": "lid_container",
          "description": "Transfer individual organoids to individual Parafilm dimples using a P200 pipette tip.",
          "amount": {
            "value": 1.0,
            "unit": "organoid"
          }
        },
        "outputs": [],
        "inputs": []
      },
      {
        "id": "s18",
        "op": "annotate",
        "params": {
          "note": "Remove excess media from the dimples using a P200 pipette under the stereoscope."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s19",
        "op": "transfer",
        "params": {
          "source": "mg_sample",
          "destination": "lid_container",
          "description": "Draw up 30 µL thawed MG with a cold P200 tip and add MG on top of the organoid in the Parafilm dimple.",
          "amount": {
            "value": 30.0,
            "unit": "µL"
          }
        },
        "outputs": [],
        "inputs": []
      },
      {
        "id": "s20",
        "op": "annotate",
        "params": {
          "note": "Reposition the organoid into the center of the MG droplet and remove bubbles if introduced."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s21",
        "op": "run_device",
        "params": {
          "device_kind": "incubator",
          "device_ref": "incubator",
          "program": {
            "program_name": "30min_37C"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s22",
        "op": "annotate",
        "params": {
          "note": "Retrieve the dish from the incubator, bring it into the biosafety cabinet, detach the Parafilm from the lid, and pop out the dimples with an ethanol‑disinfected gloved finger."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s23",
        "op": "transfer",
        "params": {
          "source": "dish_container",
          "destination": "lid_container",
          "description": "Spray medium onto each MG droplet to cause the droplet to slide into the media in the plate.",
          "amount": {
            "value": 1.0,
            "unit": "mL"
          }
        },
        "outputs": [],
        "inputs": []
      },
      {
        "id": "s24",
        "op": "annotate",
        "params": {
          "note": "Repeat until all organoids are in the plate."
        },
        "inputs": [],
        "outputs": []
      }
    ],
