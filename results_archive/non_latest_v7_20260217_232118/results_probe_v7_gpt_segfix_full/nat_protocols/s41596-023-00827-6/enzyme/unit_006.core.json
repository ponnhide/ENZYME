    "steps": [
      {
        "id": "s1",
        "op": "annotate",
        "params": {
          "note": "Remove the insert from the 24-well plate, invert it and gently peel off the membrane from the bottom of the insert using forceps."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s2",
        "op": "annotate",
        "params": {
          "note": "Place the insert with the membrane removed over the mouth of an open, empty 15 ml centrifuge tube."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s3",
        "op": "transfer",
        "params": {
          "source_material_id": "m1",
          "destination_container_id": "c2",
          "amount": {
            "value": 1.0,
            "unit": "ml"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s4",
        "op": "annotate",
        "params": {
          "note": "Close the 15 ml centrifuge tube and immediately place on ice to depolymerize the Matrigel."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s5",
        "op": "manipulate",
        "params": {
          "action_kind": "mix_gently",
          "description": "Every 20 min, gently shake the tube to check if the Matrigel has dissolved and place it back on ice."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s6",
        "op": "transfer",
        "params": {
          "source_material_id": "m2",
          "destination_container_id": "c2",
          "amount": {
            "value": 10.0,
            "unit": "ml"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s7",
        "op": "run_device",
        "params": {
          "device_kind": "centrifuge",
          "device_ref": "e2",
          "program": {
            "program_name": "200g_5min"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s8",
        "op": "annotate",
        "params": {
          "note": "Aspirate the wash medium and cell recovery reagent from the tube."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s9",
        "op": "transfer",
        "params": {
