        "inputs": [],
        "outputs": []
      },
      {
        "id": "s17",
        "op": "run_device",
        "params": {
          "device_kind": "incubator",
          "program": {
            "program_name": "culture_24h",
            "duration": "24 h",
            "temperature": "37 C",
            "CO2": "5%"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s18",
        "op": "transfer",
        "params": {
          "source_sample_ref": "s_organoid",
          "dest_container_ref": "c4",
          "tool_ref": "c6",
          "note": "Collect CTB‑injected organoids in sterile 1 ml tube",
          "amount": {
            "value": "unspecified",
            "unit": "unspecified"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s19",
        "op": "annotate",
        "params": {
          "note": "Carefully remove any carried‑over medium with a 200 µl tip without disturbing organoids"
