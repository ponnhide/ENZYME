        "inputs": [],
        "outputs": []
      },
      {
        "id": "s20",
        "op": "annotate",
        "params": {
          "note": "Wash organoids once with 1 ml prewarmed DMEM/F12 for 3 min and remove the DMEM/F12"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s21",
        "op": "run_device",
        "params": {
          "device_kind": "incubator",
          "program": {
            "program_name": "PFA_fixation",
            "duration": "30-60 min",
            "temperature": "RT"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s22",
        "op": "annotate",
        "params": {
          "note": "Clear tissue by sequential dehydration in graded EtOH series (30%, 50%, 75%, 100%) shaking 30 min at 4 °C, protected from light"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s23",
        "op": "annotate",
        "params": {
          "note": "Clear organoids with ethyl cinnamate (ECI) for 30 min at RT, protected from light"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s24",
        "op": "transfer",
        "params": {
          "source_sample_ref": "s_organoid",
          "dest_container_ref": "c5",
          "tool_ref": "c6",
          "note": "Place organoid in ECI with cut 1,000 µl tip into coverslip bottom µ‑slide, protected from light",
          "amount": {
            "value": "unspecified",
            "unit": "unspecified"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s25",
        "op": "annotate",
        "params": {
          "note": "Organoids can be stored in ECI at 4 °C protected from light up to 2 weeks"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s26",
        "op": "annotate",
        "params": {
