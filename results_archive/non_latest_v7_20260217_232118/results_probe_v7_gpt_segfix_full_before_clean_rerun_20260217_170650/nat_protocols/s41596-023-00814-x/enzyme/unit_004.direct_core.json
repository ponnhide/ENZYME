        "params": {
          "action_kind": "mix",
          "material_refs": [
            "m4",
            "m2"
          ],
          "ratio": "1:1",
          "container_ref": "c1"
        },
        "outputs": [
          {
            "kind": "sample",
            "id": "s_ctb647_dilution"
          }
        ],
        "inputs": []
      },
      {
        "id": "s5",
        "op": "manipulate",
        "params": {
          "action_kind": "spread_on_plate",
          "material_ref": "m5",
          "volume": "10 µl",
          "container_ref": "c2"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s6",
        "op": "transfer",
        "params": {
          "source_sample_ref": "s_organoid",
          "dest_container_ref": "c2",
          "tool_ref": "c6",
          "note": "Place organoids on Matrigel drop using cut 1,000 µl tip",
          "amount": {
            "value": "unspecified",
            "unit": "unspecified"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s7",
        "op": "manipulate",
        "params": {
          "action_kind": "rinse",
          "tool_ref": "c6",
          "note": "Remove excess medium, leave thin film around organoid"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s8",
        "op": "annotate",
        "params": {
          "note": "Place glass slide with organoids on stage holder of stereomicroscope under clean bench"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s9",
        "op": "annotate",
        "params": {
          "note": "Move microinjector close to organoids on microscope stage holder"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s10",
        "op": "transfer",
        "params": {
          "source_sample_ref": "s_ctb488_dilution",
          "dest_container_ref": "c6",
          "note": "Front fill capillary tip with CTB‑488",
          "amount": {
            "value": 3.0,
            "unit": "µl"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s11",
        "op": "observe",
        "params": {
          "modality": "visual",
          "features": [
            "injection of CTB‑488 into first pigmented area"
          ],
          "equipment_ref": "e3"
        },
        "outputs": [
          {
            "kind": "sample",
            "id": "d_injection1"
          }
        ],
        "inputs": []
      },
      {
        "id": "s12",
        "op": "annotate",
        "params": {
          "note": "Move microinjector tip backward and replace with a new thin glass tip"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s13",
        "op": "annotate",
        "params": {
          "note": "Add one drop of OVB medium on the organoid to prevent drying"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s14",
        "op": "transfer",
        "params": {
          "source_sample_ref": "s_ctb647_dilution",
          "dest_container_ref": "c6",
          "note": "Fill glass pipette tip with CTB‑647 for second pigmented area",
          "amount": {
            "value": 3.0,
            "unit": "µl"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s15",
        "op": "transfer",
        "params": {
          "source_sample_ref": "s_organoid",
          "dest_container_ref": "c3",
          "tool_ref": "c6",
          "note": "Transfer injected organoids into a well of a 24‑well plate for suspension culture",
          "amount": {
            "value": "unspecified",
            "unit": "unspecified"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s16",
        "op": "annotate",
        "params": {
          "note": "Expose plate to normal room light for 10 min"
