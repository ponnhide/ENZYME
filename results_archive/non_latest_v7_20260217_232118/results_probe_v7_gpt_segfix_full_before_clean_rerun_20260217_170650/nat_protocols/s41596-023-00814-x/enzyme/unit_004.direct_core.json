      {
        "id": "s2",
        "op": "annotate",
        "params": {
          "note": "Prepare fine glass capillary pipettes (e.g., with DMZ Universal Electrode Puller). Check opening by sucking and releasing DMEM/F12. Prepare at least four thin capillaries per organoid."
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s3",
        "op": "manipulate",
        "params": {
          "action_kind": "mix",
          "material_refs": [
            "m3",
            "m2"
