        "description": "0.5 mM stock solution"
      },
      {
        "id": "m_medium",
        "name": "culture medium"
      }
    ],
    "containers": [
      {
        "id": "c_plate",
        "type": "plate",
        "name": "cell culture plate"
      }
    ],
    "equipment": [
      {
        "id": "e_incubator",
        "type": "incubator",
        "name": "CO2 incubator"
      }
    ],
    "samples": [
      {
        "id": "sample_medium",
        "type": "liquid",
        "container_ref": "c_plate",
        "material_ref": "m_medium",
        "volume": "200 µl"
      }
    ],
    "data": []
  },
  "protocol": {
    "steps": [
      {
        "id": "s1",
        "op": "allocate",
        "params": {
          "allocate_kind": "sample"
        },
        "outputs": [
