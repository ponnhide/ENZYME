            "kind": "sample",
            "id": "sample_medium"
          }
        ],
        "inputs": []
      },
      {
        "id": "s2",
        "op": "transfer",
        "params": {
          "source_material": "m_idu",
          "destination_sample": "sample_medium",
          "amount": {
            "value": 10.0,
            "unit": "µl"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s3",
        "op": "manipulate",
        "params": {
          "action_kind": "mix_gently",
          "target_container": "c_plate",
          "description": "rotate plate by hand five times for 10 s"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s4",
        "op": "run_device",
        "params": {
          "device_kind": "incubator",
          "device_ref": "e_incubator",
          "program": {
            "program_name": "37C_5%CO2_25min"
          },
          "target_container": "c_plate"
        },
        "inputs": [],
        "outputs": []
      }
    ],
    "start_step_id": "s1",
    "edges": [
      {
        "from": "s1",
        "to": "s2"
      },
      {
        "from": "s2",
        "to": "s3"
      },
      {
        "from": "s3",
        "to": "s4"
      }
    ],
    "detail_level": 0
  }
