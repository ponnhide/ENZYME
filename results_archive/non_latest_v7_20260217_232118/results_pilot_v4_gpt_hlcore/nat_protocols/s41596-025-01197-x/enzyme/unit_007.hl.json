        "params": {},
        "annotations": {
          "note": "Place a box of P200 pipette tips in a -20 °C freezer for at least 15 min to cool them before use for embedding."
        }
      },
      {
        "id": "s2",
        "op": "annotate",
        "inputs": [],
        "outputs": [],
        "params": {},
        "annotations": {
          "note": "Thaw Matrigel aliquots on ice 1–2 h before embedding; 30 µL of MG will be needed for embedding each organoid."
        }
      },
