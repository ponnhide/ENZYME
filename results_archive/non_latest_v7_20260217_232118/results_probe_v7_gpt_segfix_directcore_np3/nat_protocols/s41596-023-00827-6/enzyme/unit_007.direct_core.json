        "op": "observe",
        "params": {
          "modality": "visual",
          "features": [
            "organoid_stage"
          ]
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s2",
        "op": "annotate",
        "params": {
          "note": "Thaw Matrigel (150 μl per insert) overnight as described in ‘Reagent setup’"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s3",
        "op": "allocate",
        "params": {
          "allocate_kind": "sample",
          "description": "Prepare 50 ml of ventralization medium / branching medium"
        },
        "inputs": [],
        "outputs": [
          {
            "kind": "sample",
            "id": "ventralization_medium_prepared"
          }
        ]
      },
      {
        "id": "s4",
        "op": "transfer",
        "params": {
          "source": "ventralization_medium",
          "destination": "plate96",
          "note": "Add 100 μl per well of ventralization medium to 96‑well U‑bottom plate",
          "amount": {
            "value": 100.0,
            "unit": "μl"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s5",
        "op": "observe",
        "params": {
          "modality": "visual",
          "features": [
            "folding_structure"
          ]
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s6",
        "op": "annotate",
        "params": {
          "note": "Place desired number of organoids per insert into each well of the 96‑well plate"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s7",
        "op": "annotate",
        "params": {
          "note": "Place 24‑well inserts into non‑tissue culture‑treated plates"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s8",
        "op": "transfer",
        "params": {
          "source": "matrigel",
          "destination": "insert24",
          "note": "Layer 50 μl of 100% cold Matrigel into the bottom of each insert",
          "amount": {
            "value": 50.0,
            "unit": "μl"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s9",
        "op": "annotate",
        "params": {
          "note": "Wait 5 min or until the Matrigel has solidified"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s10",
        "op": "annotate",
        "params": {
          "note": "Gently remove ventralization medium from the 96‑well plate, one well at a time"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s11",
        "op": "transfer",
        "params": {
          "source": "matrigel",
          "destination": "pipette_tip",
          "note": "Use a P1000 tip to quickly take ~30–50 μl of 100% cold Matrigel",
          "amount": {
            "value": 30.0,
            "unit": "μl"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s12",
        "op": "manipulate",
        "params": {
          "action_kind": "mix_gently",
          "target": "organoid_matrigel_mix",
          "note": "Mix the organoids with the cold Matrigel gently to avoid creating bubbles"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s13",
        "op": "manipulate",
        "params": {
          "action_kind": "spread_on_plate",
          "target": "insert24_center",
          "note": "Pick up the organoids and slowly place the organoid–Matrigel mixture in the center of an insert"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s14",
        "op": "annotate",
        "params": {
          "note": "Wait 5 min for the Matrigel to solidify and secure the organoids in the center of the insert"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s15",
        "op": "transfer",
        "params": {
          "source": "matrigel",
          "destination": "insert24",
          "note": "Add another 50 μl of 100% cold Matrigel to create a Matrigel sandwich",
          "amount": {
            "value": 50.0,
            "unit": "μl"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s16",
        "op": "run_device",
        "params": {
          "device_kind": "incubator",
          "device_ref": "incubator",
          "program": {
            "program_name": "10 min normoxic"
          },
          "note": "Put the plates in a normoxic incubator for 10 min to ensure all Matrigel has solidified"
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s17",
        "op": "transfer",
        "params": {
          "source": "ventralization_medium",
          "destination": "insert24",
          "note": "Add 500 μl per insert of ventralization medium to the inserts",
          "amount": {
            "value": 500.0,
            "unit": "μl"
          }
        },
        "inputs": [],
        "outputs": []
      },
      {
        "id": "s18",
        "op": "transfer",
        "params": {
          "source": "ventralization_medium",
          "destination": "plate24",
          "note": "Add another 500 μl per well of ventralization medium into the plate",
          "amount": {
            "value": 500.0,
            "unit": "μl"
          }
        },
        "inputs": [],
        "outputs": []
