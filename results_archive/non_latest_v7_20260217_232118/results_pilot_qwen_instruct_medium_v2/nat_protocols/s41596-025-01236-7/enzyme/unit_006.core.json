    "start_step_id": "s1",
    "steps": [
      {
        "annotations": {
          "note": "Observe iNPCs under the microscope to ensure that the cells are attached to the plate. It is common to find dead cells floating, which is normal from FACS."
        },
        "id": "s1",
        "inputs": [
          {
            "id": "s1",
            "kind": "sample"
          }
        ],
        "op": "observe",
        "outputs": [],
        "params": {
          "features": [
            "cell attachment"
          ],
          "modality": "microscope"
        }
      },
      {
        "annotations": {
          "note": "Change the medium to 1 ml of fresh hNPSR-v2 medium."
        },
        "id": "s2",
        "inputs": [
          {
            "id": "s1",
            "kind": "sample"
          }
        ],
        "op": "transfer",
        "outputs": [
          {
            "id": "s2",
            "kind": "sample"
          }
        ],
        "params": {
          "amount": {
            "unit": "ml",
            "value": 1
          },
          "destination": "12-well plate (Matrigel-coated)",
          "source": "hNPSR-v2 medium"
        }
      },
      {
        "annotations": {
          "note": "Observe iNPCs under the microscope to check confluence. iNPCs should have reached 80–90% confluence and will be ready for passaging or cryopreservation (D4 in Fig. 3a)."
        },
        "id": "s3",
        "inputs": [
          {
            "id": "s2",
            "kind": "sample"
          }
        ],
        "op": "observe",
        "outputs": [],
        "params": {
          "features": [
            "confluence"
          ],
          "modality": "microscope"
        }
      },
      {
        "annotations": {
          "note": "Some iNPC lines might grow faster. On day 3, if the medium turns yellow, change the medium with fresh hNPSR-v2 medium."
        },
        "id": "s4",
        "inputs": [],
        "op": "annotate",
        "outputs": [],
        "params": {}
      },
      {
        "annotations": {
          "note": "iNPC overgrowth (>100% confluency) leads to dramatic cell death and decreased iNPC quality."
        },
        "id": "s5",
        "inputs": [],
        "op": "annotate",
        "outputs": [],
        "params": {}
      },
      {
        "annotations": {
          "note": "Aspirate the used hNPSR-v2 medium"
        },
        "id": "s6",
        "inputs": [],
        "op": "transfer",
        "outputs": [],
        "params": {
          "amount": {
            "unit": "ml",
            "value": 1
          },
          "destination": "waste",
          "source": "used hNPSR-v2 medium"
        }
      },
      {
        "annotations": {
          "note": "Wash the cells with 1 ml of PBS."
        },
        "id": "s7",
        "inputs": [],
        "op": "transfer",
        "outputs": [],
        "params": {
          "amount": {
            "unit": "ml",
            "value": 1
          },
          "destination": "12-well plate",
          "source": "PBS"
        }
      },
      {
        "annotations": {
          "note": "Remove PBS"
        },
        "id": "s8",
        "inputs": [],
        "op": "transfer",
        "outputs": [],
        "params": {
          "amount": {
            "unit": "ml",
            "value": 1
          },
          "destination": "waste",
          "source": "PBS"
        }
      },
      {
        "annotations": {
          "note": "Add 0.5 ml of Accumax"
        },
        "id": "s9",
        "inputs": [],
        "op": "transfer",
        "outputs": [],
        "params": {
          "amount": {
            "unit": "ml",
            "value": 0.5
          },
          "destination": "12-well plate",
          "source": "Accumax"
        }
      },
      {
        "annotations": {
          "note": "Incubate at 37 °C for 8 min."
        },
        "id": "s10",
        "inputs": [],
        "op": "run_device",
        "outputs": [],
        "params": {
          "device_kind": "incubator",
          "program": {
            "duration": 8,
            "duration_unit": "min",
            "temperature": 37,
            "unit": "°C"
          }
        }
      },
      {
        "annotations": {
          "note": "Add 0.5 ml of 10% (vol/vol) FBS medium to neutralize the dissociation reaction"
        },
        "id": "s11",
        "inputs": [],
        "op": "transfer",
        "outputs": [],
        "params": {
          "amount": {
            "unit": "ml",
            "value": 0.5
          },
          "destination": "12-well plate",
          "source": "10% (vol/vol) FBS medium"
        }
      },
      {
        "annotations": {},
        "id": "s12",
        "inputs": [],
        "op": "manipulate",
        "outputs": [
          {
            "id": "s3",
            "kind": "sample"
          }
        ],
        "params": {
          "action_kind": "mix_gently",
          "cycles": 3,
          "note": "gently pipette up and down three to five times to mix the cells"
        }
      }
    ]
