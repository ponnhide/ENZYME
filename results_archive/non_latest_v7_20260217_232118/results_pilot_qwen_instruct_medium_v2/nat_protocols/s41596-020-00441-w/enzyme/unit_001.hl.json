  "protocol": {
    "steps": [
      {
        "id": "s1",
        "op": "annotate",
        "inputs": [],
        "outputs": [],
        "params": {},
        "annotations": {
          "description": "Pre-coat 24-well plates with GFR Matrigel as described in Reagent setup. The coated plates can be wrapped in Paraﬁlm and kept for ≤2 weeks at 4 oC."
        }
      },
      {
        "id": "s2",
        "op": "annotate",
        "inputs": [],
        "outputs": [],
        "params": {},
        "annotations": {
          "description": "CRITICAL STEP All tissue culture steps in this protocol should take place in a sterile environment and be performed with aseptic technique to prevent culture contamination."
        }
      },
      {
        "id": "s3",
        "op": "transfer",
        "inputs": [
          {
            "kind": "container",
            "id": "c1"
          }
        ],
        "outputs": [],
        "params": {
          "amount": {
            "value": 0,
            "unit": "ml"
          },
          "source": "well",
          "destination": "aspirate"
        },
        "annotations": {
          "description": "Remove the mTeSR1 by aspiration from the wells to be passaged (Fig. 2a)."
        }
      },
      {
        "id": "s4",
        "op": "transfer",
        "inputs": [
          {
            "kind": "container",
            "id": "c1"
          }
        ],
        "outputs": [],
        "params": {
          "amount": {
            "value": 2,
            "unit": "ml"
          },
          "source": "material",
          "destination": "well",
          "material_id": "m3"
        },
        "annotations": {
          "description": "Wash the wells with 2 ml of sterile DPBS."
        }
      },
      {
        "id": "s5",
        "op": "transfer",
        "inputs": [
          {
            "kind": "container",
            "id": "c1"
          }
        ],
        "outputs": [],
        "params": {
          "amount": {
            "value": 0,
            "unit": "ml"
          },
          "source": "well",
          "destination": "aspirate"
        },
        "annotations": {
          "description": "Aspirate the DPBS."
        }
      },
      {
        "id": "s6",
        "op": "transfer",
        "inputs": [
          {
            "kind": "container",
            "id": "c1"
          }
        ],
        "outputs": [],
        "params": {
          "amount": {
            "value": 1,
            "unit": "ml"
          },
          "source": "material",
          "destination": "well",
          "material_id": "m4"
        },
        "annotations": {
          "description": "Add 1 ml of accutase to the wells."
        }
      },
      {
        "id": "s7",
        "op": "run_device",
        "inputs": [
          {
            "kind": "container",
            "id": "c1"
          }
        ],
        "outputs": [],
        "params": {
          "device_kind": "incubator",
          "program": {
            "temperature": {
              "value": 37,
              "unit": "°C"
            },
            "duration": {
              "value": 5,
              "unit": "min"
            }
          }
        },
        "annotations": {
          "description": "Incubate at 37 oC for 5 min, allowing PSC colonies to dissociate into single cells."
        }
      },
      {
        "id": "s8",
        "op": "transfer",
        "inputs": [
          {
            "kind": "container",
            "id": "c1"
          }
        ],
        "outputs": [],
        "params": {
          "amount": {
            "value": 5,
            "unit": "ml"
          },
          "source": "material",
          "destination": "well",
          "material_id": "m5"
        },
        "annotations": {
          "description": "Add 5 ml of warm DMEM/F12 to each well to sufficiently dilute the accutase."
        }
      },
      {
        "id": "s9",
        "op": "manipulate",
        "inputs": [
          {
            "kind": "container",
            "id": "c1"
          }
        ],
        "outputs": [
          {
            "kind": "sample",
            "id": "s1"
          }
        ],
        "params": {
          "action_kind": "mix_gently",
          "repetitions": 2
        },
        "annotations": {
          "description": "Gently triturate each treated well two to three times before transferring the 6 ml of PSCs in medium to a conical tube."
        }
      },
      {
        "id": "s10",
        "op": "transfer",
        "inputs": [
          {
            "kind": "container",
            "id": "c1"
          }
        ],
        "outputs": [
          {
            "kind": "container",
            "id": "c2"
          }
        ],
        "params": {
          "amount": {
            "value": 6,
            "unit": "ml"
          },
          "source": "well",
          "destination": "container"
        },
        "annotations": {
          "description": "Repeat the process with the remaining wells until all cells are collected into the same conical tube."
        }
      },
      {
        "id": "s11",
        "op": "run_device",
        "inputs": [
          {
            "kind": "container",
            "id": "c2"
          }
        ],
        "outputs": [],
        "params": {
          "device_kind": "centrifuge",
          "program": {
            "speed": {
              "value": 1000,
              "unit": "r.p.m."
            },
            "duration": {
              "value": 3,
              "unit": "min"
            },
            "temperature": {
              "value": 4,
              "unit": "°C"
            }
          }
        },
        "annotations": {
          "description": "Centrifuge the cells at 1,000 r.p.m. for 3 min at 4 °C."
        }
      },
      {
        "id": "s12",
        "op": "transfer",
        "inputs": [
          {
            "kind": "container",
            "id": "c2"
          }
        ],
        "outputs": [],
        "params": {
          "amount": {
            "value": 0,
            "unit": "ml"
          },
          "source": "container",
          "destination": "aspirate"
        },
        "annotations": {
          "description": "Aspirate the medium from the conical tube."
        }
      },
      {
        "id": "s13",
        "op": "transfer",
        "inputs": [
          {
            "kind": "container",
            "id": "c2"
          }
        ],
        "outputs": [],
        "params": {
          "amount": {
            "value": 0,
            "unit": "ml"
          },
          "source": "material",
          "destination": "container",
          "material_id": "m2"
        },
        "annotations": {
          "description": "Add warm mTeSR1 to the conical tube."
        }
      },
      {
        "id": "s14",
        "op": "observe",
        "inputs": [
          {
            "kind": "sample",
            "id": "s1"
          }
        ],
        "outputs": [],
        "params": {
          "modality": "instrument_readout",
          "features": [
            "cell_count"
          ]
        },
        "annotations": {
          "description": "Count the cells."
        }
      },
      {
        "id": "s15",
        "op": "transfer",
        "inputs": [
          {
            "kind": "container",
            "id": "c3"
          }
        ],
        "outputs": [],
        "params": {
          "amount": {
            "value": 0,
            "unit": "ml"
          },
          "source": "container",
          "destination": "aspirate"
        },
        "annotations": {
          "description": "Aspirate the Matrigel from the prepared Matrigel-coated 24-well plate (from Step 1)."
        }
      },
      {
        "id": "s16",
        "op": "transfer",
        "inputs": [
          {
            "kind": "container",
            "id": "c2"
          }
        ],
        "outputs": [
          {
            "kind": "container",
            "id": "c3"
          }
        ],
        "params": {
          "amount": {
            "value": 0.5,
            "unit": "ml"
          },
          "source": "container",
          "destination": "well",
          "material_id": "m2"
        },
        "annotations": {
          "description": "Plate the cells at a density of 100,000–150,000 cells/well in 500 μl of mTeSR1 including 10 μM Y-27632 on a Matrigel-coated tissue culture 24-well plate."
        }
      },
      {
        "id": "s17",
        "op": "annotate",
        "inputs": [],
        "outputs": [],
        "params": {},
        "annotations": {
          "description": "Prepare cells with at least two independent wells to generate anterior and posterior gut spheroids at the same time."
        }
      },
      {
        "id": "s18",
        "op": "manipulate",
        "inputs": [
          {
            "kind": "container",
            "id": "c3"
          }
        ],
        "outputs": [],
        "params": {
          "action_kind": "mix_gently"
        },
        "annotations": {
          "description": "Place the 24-well plate in a tissue culture incubator and ensure that the single cells are evenly distributed in the wells by gently moving the 24-well plate side to side."
        }
      },
      {
        "id": "s19",
        "op": "run_device",
        "inputs": [
          {
            "kind": "container",
            "id": "c3"
          }
        ],
        "outputs": [],
        "params": {
          "device_kind": "incubator",
          "program": {
            "temperature": {
              "value": 37,
              "unit": "°C"
            },
            "co2": 5,
            "duration": {
              "value": 24,
              "unit": "h"
            }
          }
        },
        "annotations": {
          "description": "Incubate overnight in an incubator with 5% CO2 at 37 °C."
        }
      }
    ],
