    "start_step_id": "s1",
    "steps": [
      {
        "annotations": {
          "description": "Pre-coat 24-well plates with GFR Matrigel as described in Reagent setup. The coated plates can be wrapped in Paraﬁlm and kept for ≤2 weeks at 4 oC."
        },
        "id": "s1",
        "inputs": [],
        "op": "annotate",
        "outputs": [],
        "params": {}
      },
      {
        "annotations": {
          "description": "CRITICAL STEP All tissue culture steps in this protocol should take place in a sterile environment and be performed with aseptic technique to prevent culture contamination."
        },
        "id": "s2",
        "inputs": [],
        "op": "annotate",
        "outputs": [],
        "params": {}
      },
      {
        "annotations": {
          "description": "Remove the mTeSR1 by aspiration from the wells to be passaged (Fig. 2a)."
        },
        "id": "s3",
        "inputs": [
          {
            "id": "c1",
            "kind": "container"
          }
        ],
        "op": "transfer",
        "outputs": [],
        "params": {
          "amount": {
            "unit": "ml",
            "value": 0
          },
          "destination": "aspirate",
          "source": "well"
        }
      },
      {
        "annotations": {
          "description": "Wash the wells with 2 ml of sterile DPBS."
        },
        "id": "s4",
        "inputs": [
          {
            "id": "c1",
            "kind": "container"
          }
        ],
        "op": "transfer",
        "outputs": [],
        "params": {
          "amount": {
            "unit": "ml",
            "value": 2
          },
          "destination": "well",
          "material_id": "m3",
          "source": "material"
        }
      },
      {
        "annotations": {
          "description": "Aspirate the DPBS."
        },
        "id": "s5",
        "inputs": [
          {
            "id": "c1",
            "kind": "container"
          }
        ],
        "op": "transfer",
        "outputs": [],
        "params": {
          "amount": {
            "unit": "ml",
            "value": 0
          },
          "destination": "aspirate",
          "source": "well"
        }
      },
      {
        "annotations": {
          "description": "Add 1 ml of accutase to the wells."
        },
        "id": "s6",
        "inputs": [
          {
            "id": "c1",
            "kind": "container"
          }
        ],
        "op": "transfer",
        "outputs": [],
        "params": {
          "amount": {
            "unit": "ml",
            "value": 1
          },
          "destination": "well",
          "material_id": "m4",
          "source": "material"
        }
      },
      {
        "annotations": {
          "description": "Incubate at 37 oC for 5 min, allowing PSC colonies to dissociate into single cells."
        },
        "id": "s7",
        "inputs": [
          {
            "id": "c1",
            "kind": "container"
          }
        ],
        "op": "run_device",
        "outputs": [],
        "params": {
          "device_kind": "incubator",
          "program": {
            "duration": {
              "unit": "min",
              "value": 5
            },
            "temperature": {
              "unit": "°C",
              "value": 37
            }
          }
        }
      },
      {
        "annotations": {
          "description": "Add 5 ml of warm DMEM/F12 to each well to sufficiently dilute the accutase."
        },
        "id": "s8",
        "inputs": [
          {
            "id": "c1",
            "kind": "container"
          }
        ],
        "op": "transfer",
        "outputs": [],
        "params": {
          "amount": {
            "unit": "ml",
            "value": 5
          },
          "destination": "well",
          "material_id": "m5",
          "source": "material"
        }
      },
      {
        "annotations": {
          "description": "Gently triturate each treated well two to three times before transferring the 6 ml of PSCs in medium to a conical tube."
        },
        "id": "s9",
        "inputs": [
          {
            "id": "c1",
            "kind": "container"
          }
        ],
        "op": "manipulate",
        "outputs": [
          {
            "id": "s1",
            "kind": "sample"
          }
        ],
        "params": {
          "action_kind": "mix_gently",
          "repetitions": 2
        }
      },
      {
        "annotations": {
          "description": "Repeat the process with the remaining wells until all cells are collected into the same conical tube."
        },
        "id": "s10",
        "inputs": [
          {
            "id": "c1",
            "kind": "container"
          }
        ],
        "op": "transfer",
        "outputs": [
          {
            "id": "c2",
            "kind": "container"
          }
        ],
        "params": {
          "amount": {
            "unit": "ml",
            "value": 6
          },
          "destination": "container",
          "source": "well"
        }
      },
      {
        "annotations": {
          "description": "Centrifuge the cells at 1,000 r.p.m. for 3 min at 4 °C."
        },
        "id": "s11",
        "inputs": [
          {
            "id": "c2",
            "kind": "container"
          }
        ],
        "op": "run_device",
        "outputs": [],
        "params": {
          "device_kind": "centrifuge",
          "program": {
            "duration": {
              "unit": "min",
              "value": 3
            },
            "speed": {
              "unit": "r.p.m.",
              "value": 1000
            },
            "temperature": {
              "unit": "°C",
              "value": 4
            }
          }
        }
      },
      {
        "annotations": {
          "description": "Aspirate the medium from the conical tube."
        },
        "id": "s12",
        "inputs": [
          {
            "id": "c2",
            "kind": "container"
          }
        ],
        "op": "transfer",
        "outputs": [],
        "params": {
          "amount": {
            "unit": "ml",
            "value": 0
          },
          "destination": "aspirate",
          "source": "container"
        }
      },
      {
        "annotations": {
          "description": "Add warm mTeSR1 to the conical tube."
        },
        "id": "s13",
        "inputs": [
          {
            "id": "c2",
            "kind": "container"
          }
        ],
        "op": "transfer",
        "outputs": [],
        "params": {
          "amount": {
            "unit": "ml",
            "value": 0
          },
          "destination": "container",
          "material_id": "m2",
          "source": "material"
        }
      },
      {
        "annotations": {
          "description": "Count the cells."
        },
        "id": "s14",
        "inputs": [
          {
            "id": "s1",
            "kind": "sample"
          }
        ],
        "op": "observe",
        "outputs": [],
        "params": {
          "features": [
            "cell_count"
          ],
          "modality": "instrument_readout"
        }
      },
      {
        "annotations": {
          "description": "Aspirate the Matrigel from the prepared Matrigel-coated 24-well plate (from Step 1)."
        },
        "id": "s15",
        "inputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "op": "transfer",
        "outputs": [],
        "params": {
          "amount": {
            "unit": "ml",
            "value": 0
          },
          "destination": "aspirate",
          "source": "container"
        }
      },
      {
        "annotations": {
          "description": "Plate the cells at a density of 100,000–150,000 cells/well in 500 μl of mTeSR1 including 10 μM Y-27632 on a Matrigel-coated tissue culture 24-well plate."
        },
        "id": "s16",
        "inputs": [
          {
            "id": "c2",
            "kind": "container"
          }
        ],
        "op": "transfer",
        "outputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "params": {
          "amount": {
            "unit": "ml",
            "value": 0.5
          },
          "destination": "well",
          "material_id": "m2",
          "source": "container"
        }
      },
      {
        "annotations": {
          "description": "Prepare cells with at least two independent wells to generate anterior and posterior gut spheroids at the same time."
        },
        "id": "s17",
        "inputs": [],
        "op": "annotate",
        "outputs": [],
        "params": {}
      },
      {
        "annotations": {
          "description": "Place the 24-well plate in a tissue culture incubator and ensure that the single cells are evenly distributed in the wells by gently moving the 24-well plate side to side."
        },
        "id": "s18",
        "inputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "op": "manipulate",
        "outputs": [],
        "params": {
          "action_kind": "mix_gently"
        }
      },
      {
        "annotations": {
          "description": "Incubate overnight in an incubator with 5% CO2 at 37 °C."
        },
        "id": "s19",
        "inputs": [
          {
            "id": "c3",
            "kind": "container"
          }
        ],
        "op": "run_device",
        "outputs": [],
        "params": {
          "device_kind": "incubator",
          "program": {
            "co2": 5,
            "duration": {
              "unit": "h",
              "value": 24
            },
            "temperature": {
              "unit": "°C",
              "value": 37
            }
          }
        }
      }
    ]
  },
